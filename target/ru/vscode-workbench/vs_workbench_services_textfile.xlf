  <file original="src/vs/workbench/services/textfile/electron-browser/nativeTextFileService" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="fileBinaryError">
        <source xml:lang="en">File seems to be binary and cannot be opened as text</source>
        <target state="translated">Похоже, файл является двоичным, и его нельзя открыть как текстовый.</target>
      </trans-unit>
      <trans-unit id="fileReadOnlyError">
        <source xml:lang="en">File is Read Only</source>
        <target state="translated">Файл доступен только для чтения</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/browser/textFileService" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="fileBinaryError">
        <source xml:lang="en">File seems to be binary and cannot be opened as text</source>
        <target state="translated">Похоже, файл является двоичным, и его нельзя открыть как текстовый.</target>
      </trans-unit>
      <trans-unit id="confirmOverwrite">
        <source xml:lang="en">'{0}' already exists. Do you want to replace it?</source>
        <target state="translated">"{0}" уже существует. Вы хотите заменить его?</target>
      </trans-unit>
      <trans-unit id="irreversible">
        <source xml:lang="en">A file or folder with the name '{0}' already exists in the folder '{1}'. Replacing it will overwrite its current contents.</source>
        <target state="translated">Файл или папка с именем "{0}" уже существует в папке "{1}". Если выполнить замену, текущее содержимое файла или папки будет перезаписано.</target>
      </trans-unit>
      <trans-unit id="replaceButtonLabel">
        <source xml:lang="en">&amp;&amp;Replace</source>
        <target state="translated">&amp;&amp;Заменить</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/common/textFileEditorModel" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="saveFileFirst">
        <source xml:lang="en">The file is dirty. Please save it first before reopening it with another encoding.</source>
        <target state="translated">Файл изменен. Сохраните его, прежде чем открыть его вновь в другой кодировке.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/common/textFileEditorModelManager" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="genericSaveError">
        <source xml:lang="en">Failed to save '{0}': {1}</source>
        <target state="translated">Не удалось сохранить "{0}": {1}</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/common/textFileSaveParticipant" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="saveParticipants">
        <source xml:lang="en">Saving '{0}'</source>
        <target state="translated">Идет сохранение "{0}"</target>
      </trans-unit>
    </body>
  </file>
