  <file original="src/vs/workbench/services/textfile/browser/textFileService" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="fileBinaryError">
        <source xml:lang="en">File seems to be binary and cannot be opened as text</source>
        <target state="translated">El archivo parece ser binario y no se puede abrir como texto</target>
      </trans-unit>
      <trans-unit id="confirmOverwrite">
        <source xml:lang="en">'{0}' already exists. Do you want to replace it?</source>
        <target state="translated">"{0}" ya existe. ¿Desea reemplazarlo?</target>
      </trans-unit>
      <trans-unit id="irreversible">
        <source xml:lang="en">A file or folder with the name '{0}' already exists in the folder '{1}'. Replacing it will overwrite its current contents.</source>
        <target state="translated">Ya existe un archivo o carpeta con el nombre "{0}" en la carpeta "{1}". Reemplazarlo sobrescribirá su contenido actual.</target>
      </trans-unit>
      <trans-unit id="replaceButtonLabel">
        <source xml:lang="en">&amp;&amp;Replace</source>
        <target state="translated">&amp;&amp;Reemplazar</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/common/textFileEditorModel" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="saveFileFirst">
        <source xml:lang="en">The file is dirty. Please save it first before reopening it with another encoding.</source>
        <target state="translated">Este es un archivo con modificaciones. Guárdelo antes de volver a abrirlo con otra codificación.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/services/textfile/common/textFileEditorModelManager" source-language="en" target-language="es" datatype="plaintext">
