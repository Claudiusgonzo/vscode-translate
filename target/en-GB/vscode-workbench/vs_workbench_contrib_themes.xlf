      </trans-unit>
      <trans-unit id="preferences">
        <source xml:lang="en">Preferences</source>
        <target state="translated">Preferences</target>
      </trans-unit>
      <trans-unit id="developer">
        <source xml:lang="en">Developer</source>
        <target state="translated">Developer</target>
      </trans-unit>
      <trans-unit id="miSelectColorTheme">
        <source xml:lang="en">&amp;&amp;Color Theme</source>
        <target state="translated">&amp;&amp;Colour Theme</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miSelectIconTheme">
        <source xml:lang="en">File &amp;&amp;Icon Theme</source>
        <target state="translated">File &amp;&amp;Icon Theme</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="themes.selectIconTheme.label">
        <source xml:lang="en">File Icon Theme</source>
        <target state="translated">File Icon Theme</target>
      </trans-unit>
