<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target state="translated">Ajouter l'action "Ouvrir avec %1" au menu contextuel de fichier de l'Explorateur Windows</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target state="translated">Ajouter l'action "Ouvrir avec %1" au menu contextuel de répertoire de l'Explorateur Windows</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target state="translated">Inscrire %1 en tant qu'éditeur pour les types de fichier pris en charge</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target state="translated">Ajouter à PATH (nécessite un redémarrage de l'interpréteur de commande)</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target state="translated">Exécuter %1 après l'installation</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target state="translated">Autre :</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target state="translated">Fichier source %1</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target state="translated">Ouvr&amp;ir avec %1</target>
      </trans-unit>
    </body>
  </file>
</xliff>