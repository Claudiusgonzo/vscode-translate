  <file original="src/vs/workbench/contrib/update/browser/update" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotes">
        <source xml:lang="en">Release Notes</source>
        <target state="translated">Notes de publication</target>
      </trans-unit>
      <trans-unit id="showReleaseNotes">
        <source xml:lang="en">Show Release Notes</source>
        <target state="translated">Afficher les notes de publication</target>
      </trans-unit>
      <trans-unit id="read the release notes">
        <source xml:lang="en">Welcome to {0} v{1}! Would you like to read the Release Notes?</source>
        <target state="translated">Bienvenue dans {0} v{1} ! Voulez-vous lire les notes de publication ?</target>
      </trans-unit>
      <trans-unit id="licenseChanged">
        <source xml:lang="en">Our license terms have changed, please click [here]({0}) to go through them.</source>
        <target state="translated">Nos termes du contrat de licence ont changé. Veuillez cliquer [ici]({0}) pour les consulter.</target>
      </trans-unit>
      <trans-unit id="updateIsReady">
        <source xml:lang="en">New {0} update available.</source>
        <target state="translated">Nouvelle mise à jour de {0} disponible.</target>
      </trans-unit>
      <trans-unit id="checkingForUpdates">
        <source xml:lang="en">Checking for Updates...</source>
        <target state="translated">Recherche de mises à jour...</target>
      </trans-unit>
      <trans-unit id="update service">
        <source xml:lang="en">Update Service</source>
        <target state="translated">Service de mise à jour</target>
      </trans-unit>
      <trans-unit id="noUpdatesAvailable">
        <source xml:lang="en">There are currently no updates available.</source>
        <target state="translated">Aucune mise à jour n'est disponible actuellement.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">OK</target>
      </trans-unit>
      <trans-unit id="thereIsUpdateAvailable">
        <source xml:lang="en">There is an available update.</source>
        <target state="translated">Une mise à jour est disponible.</target>
      </trans-unit>
      <trans-unit id="download update">
        <source xml:lang="en">Download Update</source>
        <target state="translated">Télécharger la mise à jour</target>
      </trans-unit>
      <trans-unit id="later">
        <source xml:lang="en">Later</source>
        <target state="translated">Plus tard</target>
      </trans-unit>
      <trans-unit id="updateAvailable">
        <source xml:lang="en">There's an update available: {0} {1}</source>
        <target state="translated">Une mise à jour est disponible : {0} {1}</target>
      </trans-unit>
      <trans-unit id="installUpdate">
        <source xml:lang="en">Install Update</source>
        <target state="translated">Installer la mise à jour</target>
      </trans-unit>
      <trans-unit id="updateInstalling">
        <source xml:lang="en">{0} {1} is being installed in the background; we'll let you know when it's done.</source>
        <target state="translated">{0} {1} est installé en tâche de fond ; Nous vous ferons savoir quand c’est fini.</target>
      </trans-unit>
      <trans-unit id="updateNow">
        <source xml:lang="en">Update Now</source>
        <target state="translated">Mettre à jour maintenant</target>
      </trans-unit>
      <trans-unit id="updateAvailableAfterRestart">
        <source xml:lang="en">Restart {0} to apply the latest update.</source>
        <target state="translated">Redémarrer {0} pour appliquer la dernière mise à jour.</target>
      </trans-unit>
      <trans-unit id="checkForUpdates">
        <source xml:lang="en">Check for Updates...</source>
        <target state="translated">Rechercher les mises à jour...</target>
      </trans-unit>
      <trans-unit id="DownloadingUpdate">
        <source xml:lang="en">Downloading Update...</source>
        <target state="translated">Téléchargement de la mise à jour...</target>
      </trans-unit>
      <trans-unit id="installUpdate...">
        <source xml:lang="en">Install Update...</source>
        <target state="translated">Installer la mise à jour...</target>
      </trans-unit>
      <trans-unit id="installingUpdate">
        <source xml:lang="en">Installing Update...</source>
        <target state="translated">Installation de la mise à jour...</target>
      </trans-unit>
      <trans-unit id="restartToUpdate">
        <source xml:lang="en">Restart to Update (1)</source>
        <target state="translated">Redémarrer pour mettre à jour (1)</target>
      </trans-unit>
    </body>
  </file>
