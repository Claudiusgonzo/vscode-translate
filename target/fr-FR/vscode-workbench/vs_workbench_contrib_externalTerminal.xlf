  <file original="src/vs/workbench/contrib/externalTerminal/node/externalTerminalService" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="console.title">
        <source xml:lang="en">VS Code Console</source>
        <target state="translated">Console VS Code</target>
      </trans-unit>
      <trans-unit id="mac.terminal.script.failed">
        <source xml:lang="en">Script '{0}' failed with exit code {1}</source>
        <target state="translated">Échec du script '{0}'. Code de sortie : {1}</target>
      </trans-unit>
      <trans-unit id="mac.terminal.type.not.supported">
        <source xml:lang="en">'{0}' not supported</source>
        <target state="translated">'{0}' non pris en charge</target>
      </trans-unit>
      <trans-unit id="press.any.key">
        <source xml:lang="en">Press any key to continue...</source>
        <target state="translated">Appuyez sur une touche pour continuer...</target>
      </trans-unit>
      <trans-unit id="linux.term.failed">
        <source xml:lang="en">'{0}' failed with exit code {1}</source>
        <target state="translated">Échec de '{0}'. Code de sortie : {1}</target>
      </trans-unit>
      <trans-unit id="ext.term.app.not.found">
        <source xml:lang="en">can't find terminal application '{0}'</source>
        <target state="translated">application de terminal '{0}' introuvable</target>
      </trans-unit>
      <trans-unit id="terminalConfigurationTitle">
        <source xml:lang="en">External Terminal</source>
        <target state="translated">Terminal externe</target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.integrated">
        <source xml:lang="en">Use VS Code's integrated terminal.</source>
        <target state="translated">Utiliser le terminal intégré de VS Code.</target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.external">
        <source xml:lang="en">Use the configured external terminal.</source>
        <target state="translated">Utiliser le terminal externe configuré.</target>
      </trans-unit>
      <trans-unit id="explorer.openInTerminalKind">
        <source xml:lang="en">Customizes what kind of terminal to launch.</source>
        <target state="translated">Personnalise le type de terminal à lancer.</target>
      </trans-unit>
      <trans-unit id="terminal.external.windowsExec">
        <source xml:lang="en">Customizes which terminal to run on Windows.</source>
        <target state="translated">Personnalise le terminal à exécuter sur Windows.</target>
      </trans-unit>
      <trans-unit id="terminal.external.osxExec">
        <source xml:lang="en">Customizes which terminal application to run on macOS.</source>
        <target state="translated">Personnalise l’application de terminal à exécuter sur macOS.</target>
      </trans-unit>
      <trans-unit id="terminal.external.linuxExec">
        <source xml:lang="en">Customizes which terminal to run on Linux.</source>
        <target state="translated">Personnalise le terminal à exécuter sur Linux.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/externalTerminal/browser/externalTerminal.contribution" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="globalConsoleAction">
        <source xml:lang="en">Open New External Terminal</source>
        <target state="translated">Ouvrir un nouveau terminal externe</target>
      </trans-unit>
      <trans-unit id="scopedConsoleAction">
        <source xml:lang="en">Open in Terminal</source>
        <target state="translated">Ouvrir dans Terminal</target>
      </trans-unit>
    </body>
  </file>
