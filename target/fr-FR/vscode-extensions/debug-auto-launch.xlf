<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/debug-auto-launch/package" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Node Debug Auto-attach</source>
        <target state="translated">Attachement automatique du débogage de nœud</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Helper for auto-attach feature when node-debug extensions are not active.</source>
        <target state="translated">Assistance pour la fonctionnalité d'attachement automatique quand les extensions de débogage de nœud ne sont pas actives. </target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.description">
        <source xml:lang="en">Automatically attach node debugger when node.js was launched in debug mode from integrated terminal.</source>
        <target state="translated">Attacher automatiquement le débogueur de nœud à la suite du lancement de node.js en mode débogage à partir du terminal intégré.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.disabled.description">
        <source xml:lang="en">Auto attach is disabled and not shown in status bar.</source>
        <target state="translated">L'attachement automatique est désactivé et n'est pas affiché dans la barre d’état.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.on.description">
        <source xml:lang="en">Auto attach is active.</source>
        <target state="translated">L'attachement automatique est actif.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.off.description">
        <source xml:lang="en">Auto attach is inactive.</source>
        <target state="translated">L'attachement automatique est inactif.</target>
      </trans-unit>
      <trans-unit id="toggle.auto.attach">
        <source xml:lang="en">Toggle Auto Attach</source>
        <target state="translated">Activer/désactiver l'Attachement automatique</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/debug-auto-launch/dist/extension" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="status.text.auto.attach.on">
        <source xml:lang="en">Auto Attach: On</source>
        <target state="translated">Attachement automatique : Activé</target>
      </trans-unit>
      <trans-unit id="status.text.auto.attach.off">
        <source xml:lang="en">Auto Attach: Off</source>
        <target state="translated">Attachement automatique : Désactivé</target>
      </trans-unit>
      <trans-unit id="status.tooltip.auto.attach">
        <source xml:lang="en">Automatically attach to node.js processes in debug mode</source>
        <target state="translated">Attacher automatiquement à node.js les processus en mode débogage</target>
      </trans-unit>
    </body>
  </file>
</xliff>