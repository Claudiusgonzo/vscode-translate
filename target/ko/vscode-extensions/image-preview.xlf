    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Image Preview</source>
        <target state="translated">이미지 미리 보기</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Provides VS Code's built-in image preview</source>
        <target state="translated">VS Code의 기본 제공 이미지 미리 보기를 제공함</target>
      </trans-unit>
      <trans-unit id="customEditors.displayName">
        <source xml:lang="en">Image Preview</source>
        <target state="translated">이미지 미리 보기</target>
      </trans-unit>
