      <trans-unit id="miReleaseNotes">
        <source xml:lang="en">&amp;&amp;Release Notes</source>
        <target state="translated">릴리스 정보(&amp;&amp;R)</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
    </body>
