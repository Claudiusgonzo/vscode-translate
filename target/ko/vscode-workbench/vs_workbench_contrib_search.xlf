  <file original="src/vs/workbench/contrib/search/browser/searchView" source-language="en" target-language="ko" datatype="plaintext">
    <body>
      <trans-unit id="searchCanceled">
        <source xml:lang="en">Search was canceled before any results could be found - </source>
        <target state="translated">결과를 찾기 이전에 검색이 취소되었습니다. -</target>
      </trans-unit>
      <trans-unit id="searchView">
        <source xml:lang="en">Search</source>
        <target state="translated">검색</target>
      </trans-unit>
      <trans-unit id="moreSearch">
        <source xml:lang="en">Toggle Search Details</source>
        <target state="translated">검색 세부 정보 설정/해제</target>
      </trans-unit>
      <trans-unit id="searchScope.includes">
        <source xml:lang="en">files to include</source>
        <target state="translated">포함할 파일</target>
      </trans-unit>
      <trans-unit id="label.includes">
        <source xml:lang="en">Search Include Patterns</source>
        <target state="translated">패턴 포함 검색</target>
      </trans-unit>
      <trans-unit id="searchScope.excludes">
        <source xml:lang="en">files to exclude</source>
        <target state="translated">제외할 파일</target>
      </trans-unit>
      <trans-unit id="label.excludes">
        <source xml:lang="en">Search Exclude Patterns</source>
        <target state="translated">패턴 제외 검색</target>
      </trans-unit>
      <trans-unit id="replaceAll.confirmation.title">
        <source xml:lang="en">Replace All</source>
        <target state="translated">모두 바꾸기</target>
      </trans-unit>
      <trans-unit id="replaceAll.confirm.button">
        <source xml:lang="en">&amp;&amp;Replace</source>
        <target state="translated">바꾸기(&amp;&amp;R)</target>
      </trans-unit>
      <trans-unit id="replaceAll.occurrence.file.message">
        <source xml:lang="en">Replaced {0} occurrence across {1} file with '{2}'.</source>
        <target state="translated">{1}개 파일에서 {0}개를 '{2}'(으)로 바꿨습니다.</target>
      </trans-unit>
      <trans-unit id="removeAll.occurrence.file.message">
        <source xml:lang="en">Replaced {0} occurrence across {1} file.</source>
        <target state="translated">{1} 파일에서 {0} 발생을 바꿨습니다.</target>
      </trans-unit>
      <trans-unit id="replaceAll.occurrence.files.message">
        <source xml:lang="en">Replaced {0} occurrence across {1} files with '{2}'.</source>
        <target state="translated">{1}개 파일에서 {0}개를 '{2}'(으)로 바꿨습니다.</target>
      </trans-unit>
      <trans-unit id="removeAll.occurrence.files.message">
        <source xml:lang="en">Replaced {0} occurrence across {1} files.</source>
        <target state="translated">{1}개 파일에서 {0}개를 바꿨습니다.</target>
      </trans-unit>
      <trans-unit id="replaceAll.occurrences.file.message">
        <source xml:lang="en">Replaced {0} occurrences across {1} file with '{2}'.</source>
        <target state="translated">{1}개 파일에서 {0}개를 '{2}'(으)로 바꿨습니다.</target>
      </trans-unit>
      <trans-unit id="removeAll.occurrences.file.message">
        <source xml:lang="en">Replaced {0} occurrences across {1} file.</source>
        <target state="translated">{1} 파일에서 {0} 발생을 바꿨습니다.</target>
      </trans-unit>
