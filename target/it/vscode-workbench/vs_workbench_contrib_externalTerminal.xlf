<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/externalTerminal/node/externalTerminalService" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="console.title">
        <source xml:lang="en">VS Code Console</source>
        <target state="translated">Console di Visual Studio Code</target>
      </trans-unit>
      <trans-unit id="mac.terminal.script.failed">
        <source xml:lang="en">Script '{0}' failed with exit code {1}</source>
        <target state="translated">Lo script '{0}' non è riuscito. Codice di uscita: {1}</target>
      </trans-unit>
      <trans-unit id="mac.terminal.type.not.supported">
        <source xml:lang="en">'{0}' not supported</source>
        <target state="translated">'{0}' non supportato</target>
      </trans-unit>
      <trans-unit id="press.any.key">
        <source xml:lang="en">Press any key to continue...</source>
        <target state="translated">Premere un tasto qualsiasi per continuare...</target>
      </trans-unit>
      <trans-unit id="linux.term.failed">
        <source xml:lang="en">'{0}' failed with exit code {1}</source>
        <target state="translated">'{0}' non riuscito. Codice di uscita: {1}</target>
      </trans-unit>
      <trans-unit id="ext.term.app.not.found">
        <source xml:lang="en">can't find terminal application '{0}'</source>
        <target state="translated">non è possibile trovare l'applicazione di terminale '{0}'</target>
      </trans-unit>
      <trans-unit id="terminalConfigurationTitle">
        <source xml:lang="en">External Terminal</source>
        <target state="translated">Terminale esterno</target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.integrated">
        <source xml:lang="en">Use VS Code's integrated terminal.</source>
        <target state="translated">Usare il terminale integrato di VS Code. </target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.external">
        <source xml:lang="en">Use the configured external terminal.</source>
        <target state="translated">Usare il terminale esterno configurato. </target>
      </trans-unit>
      <trans-unit id="explorer.openInTerminalKind">
        <source xml:lang="en">Customizes what kind of terminal to launch.</source>
        <target state="translated">Personalizza il tipo di terminale da avviare.</target>
      </trans-unit>
      <trans-unit id="terminal.external.windowsExec">
        <source xml:lang="en">Customizes which terminal to run on Windows.</source>
        <target state="translated">Personalizza il terminale da eseguire in Windows.</target>
      </trans-unit>
      <trans-unit id="terminal.external.osxExec">
        <source xml:lang="en">Customizes which terminal application to run on macOS.</source>
        <target state="translated">Personalizza l'applicazione di terminale da eseguire in macOS.</target>
      </trans-unit>
      <trans-unit id="terminal.external.linuxExec">
        <source xml:lang="en">Customizes which terminal to run on Linux.</source>
        <target state="translated">Personalizza il terminale da eseguire in Linux.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/externalTerminal/browser/externalTerminal.contribution" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="globalConsoleAction">
        <source xml:lang="en">Open New External Terminal</source>
        <target state="translated">Apri nuovo terminale esterno</target>
      </trans-unit>
      <trans-unit id="scopedConsoleAction">
        <source xml:lang="en">Open in Terminal</source>
        <target state="translated">Apri nel terminale</target>
      </trans-unit>
    </body>
  </file>
