  <file original="src/vs/workbench/contrib/format/browser/formatActionsNone" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document</source>
        <target state="translated">Formatta documento</target>
      </trans-unit>
      <trans-unit id="no.provider">
        <source xml:lang="en">There is no formatter for '{0}' files installed.</source>
        <target state="translated">Non è installato alcun formattatore per i file '{0}'.</target>
      </trans-unit>
      <trans-unit id="install.formatter">
        <source xml:lang="en">Install Formatter...</source>
        <target state="translated">Installa formattatore...</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/format/browser/formatActionsMultiple" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="nullFormatterDescription">
        <source xml:lang="en">None</source>
        <target state="translated">Nessuno</target>
      </trans-unit>
      <trans-unit id="miss">
        <source xml:lang="en">Extension '{0}' cannot format '{1}'</source>
        <target state="translated">L'estensione '{0}' non può formattare '{1}'</target>
      </trans-unit>
      <trans-unit id="config.needed">
        <source xml:lang="en">There are multiple formatters for '{0}' files. Select a default formatter to continue.</source>
        <target state="translated">Sono disponibili più formattatori per i file '{0}'. Selezionare un formattatore predefinito per continuare.</target>
      </trans-unit>
      <trans-unit id="config.bad">
        <source xml:lang="en">Extension '{0}' is configured as formatter but not available. Select a different default formatter to continue.</source>
        <target state="translated">L'estensione '{0}' è configurata come formattatore, ma non è disponibile. Per continuare, selezionare un altro formattatore predefinito.</target>
      </trans-unit>
      <trans-unit id="do.config">
        <source xml:lang="en">Configure...</source>
        <target state="translated">Configura...</target>
      </trans-unit>
      <trans-unit id="select">
        <source xml:lang="en">Select a default formatter for '{0}' files</source>
        <target state="translated">Selezionare un formattatore predefinito per i file '{0}'</target>
      </trans-unit>
      <trans-unit id="formatter.default">
        <source xml:lang="en">Defines a default formatter which takes precedence over all other formatter settings. Must be the identifier of an extension contributing a formatter.</source>
        <target state="translated">Consente di definire un formattatore predefinito che ha la precedenza su tutte le altre impostazioni di formattatore. Deve essere l'identificatore di un'estensione che contribuisce a un formattatore.</target>
      </trans-unit>
      <trans-unit id="def">
        <source xml:lang="en">(default)</source>
        <target state="translated">(Predefinita)</target>
      </trans-unit>
      <trans-unit id="config">
        <source xml:lang="en">Configure Default Formatter...</source>
        <target state="translated">Configura il formattatore predefinito...</target>
      </trans-unit>
      <trans-unit id="format.placeHolder">
        <source xml:lang="en">Select a formatter</source>
        <target state="translated">Selezionare un formattatore</target>
      </trans-unit>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document With...</source>
        <target state="translated">Formatta documento con...</target>
      </trans-unit>
      <trans-unit id="formatSelection.label.multiple">
        <source xml:lang="en">Format Selection With...</source>
        <target state="translated">Formatta selezione con...</target>
      </trans-unit>
    </body>
  </file>
