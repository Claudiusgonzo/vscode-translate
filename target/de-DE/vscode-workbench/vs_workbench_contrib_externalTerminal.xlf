<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/externalTerminal/node/externalTerminalService" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="console.title">
        <source xml:lang="en">VS Code Console</source>
        <target state="translated">VS Code-Konsole</target>
      </trans-unit>
      <trans-unit id="mac.terminal.script.failed">
        <source xml:lang="en">Script '{0}' failed with exit code {1}</source>
        <target state="translated">Fehler bei Skript "{0}" mit Exitcode {1}.</target>
      </trans-unit>
      <trans-unit id="mac.terminal.type.not.supported">
        <source xml:lang="en">'{0}' not supported</source>
        <target state="translated">{0}" wird nicht unterstützt.</target>
      </trans-unit>
      <trans-unit id="press.any.key">
        <source xml:lang="en">Press any key to continue...</source>
        <target state="translated">Drücken Sie eine beliebige Taste, um fortzufahren...</target>
      </trans-unit>
      <trans-unit id="linux.term.failed">
        <source xml:lang="en">'{0}' failed with exit code {1}</source>
        <target state="translated">Fehler bei "{0}" mit Exitcode {1}.</target>
      </trans-unit>
      <trans-unit id="ext.term.app.not.found">
        <source xml:lang="en">can't find terminal application '{0}'</source>
        <target state="translated">Terminalanwendung "{0}" konnte nicht gefunden werden.</target>
      </trans-unit>
      <trans-unit id="terminalConfigurationTitle">
        <source xml:lang="en">External Terminal</source>
        <target state="translated">Externes Terminal</target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.integrated">
        <source xml:lang="en">Use VS Code's integrated terminal.</source>
        <target state="translated">Das integrierte Terminal von Visual Studio Code verwenden</target>
      </trans-unit>
      <trans-unit id="terminal.explorerKind.external">
        <source xml:lang="en">Use the configured external terminal.</source>
        <target state="translated">Das konfigurierte externe Terminal verwenden</target>
      </trans-unit>
      <trans-unit id="explorer.openInTerminalKind">
        <source xml:lang="en">Customizes what kind of terminal to launch.</source>
        <target state="translated">Passt an, welches Terminal ausgeführt werden soll.</target>
      </trans-unit>
      <trans-unit id="terminal.external.windowsExec">
        <source xml:lang="en">Customizes which terminal to run on Windows.</source>
        <target state="translated">Passt an, welches Terminal für Windows ausgeführt werden soll.</target>
      </trans-unit>
      <trans-unit id="terminal.external.osxExec">
        <source xml:lang="en">Customizes which terminal application to run on macOS.</source>
        <target state="translated">Passt an, welche Terminalanwendung unter macOS ausgeführt werden soll.</target>
      </trans-unit>
      <trans-unit id="terminal.external.linuxExec">
        <source xml:lang="en">Customizes which terminal to run on Linux.</source>
        <target state="translated">Passt an, welches Terminal unter Linux ausgeführt werden soll.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/externalTerminal/browser/externalTerminal.contribution" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="globalConsoleAction">
        <source xml:lang="en">Open New External Terminal</source>
        <target state="translated">Neues externes Terminal öffnen</target>
      </trans-unit>
      <trans-unit id="scopedConsoleAction">
        <source xml:lang="en">Open in Terminal</source>
        <target state="translated">In Terminal öffnen</target>
      </trans-unit>
    </body>
  </file>
