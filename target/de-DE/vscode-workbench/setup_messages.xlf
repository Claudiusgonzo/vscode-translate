<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target state="translated">Aktion "Mit %1 öffnen" dem Dateikontextmenü von Windows-Explorer hinzufügen</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target state="translated">Aktion "Mit %1 öffnen" dem Verzeichniskontextmenü von Windows-Explorer hinzufügen</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target state="translated">%1 als Editor für unterstützte Dateitypen registrieren</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target state="translated">Zu PATH hinzufügen (Neustart der Shell erforderlich)</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target state="translated">%1 nach der Installation ausführen</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target state="translated">Andere:</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target state="translated">%1-Quelldatei</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target state="translated">Mit %1 öffnen</target>
      </trans-unit>
    </body>
  </file>
</xliff>