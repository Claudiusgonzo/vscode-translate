    <body>
      <trans-unit id="remote">
        <source xml:lang="en">Remote</source>
        <target state="translated">Remote</target>
      </trans-unit>
      <trans-unit id="remote.downloadExtensionsLocally">
        <source xml:lang="en">When enabled extensions are downloaded locally and installed on remote.</source>
        <target state="translated">Wenn aktiviert, werden Erweiterungen lokal heruntergeladen und auf dem Remotecomputer installiert</target>
      </trans-unit>
      <trans-unit id="remote.restoreForwardedPorts">
        <source xml:lang="en">Restores the ports you forwarded in a workspace.</source>
        <target state="translated">Stellt die Ports wieder her, die Sie in einem Arbeitsbereich weitergeleitet haben.</target>
      </trans-unit>
    </body>
