  <file original="src/vs/workbench/contrib/snippets/browser/snippets.contribution" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="snippetSchema.json.default">
        <source xml:lang="en">Empty snippet</source>
        <target state="translated">Leerer Codeausschnitt</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json">
        <source xml:lang="en">User snippet configuration</source>
        <target state="translated">Benutzerkonfiguration des Codeausschnitts</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.prefix">
        <source xml:lang="en">The prefix to used when selecting the snippet in intellisense</source>
        <target state="translated">Das Präfix, das beim Auswählen des Codeausschnitts in IntelliSense verwendet werden soll.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.body">
        <source xml:lang="en">The snippet content. Use '$1', '${1:defaultText}' to define cursor positions, use '$0' for the final cursor position. Insert variable values with '${varName}' and '${varName:defaultText}', e.g. 'This is file: $TM_FILENAME'.</source>
        <target state="translated">Der Inhalt des Codeausschnitts. Verwenden Sie "$1", "${1:defaultText}" zum Definieren der Cursorpositionen, und legen Sie die endgültige Cursorposition mit "$0" fest. Fügen Sie Variablenwerte mit "${varName}" und "${varName:defaultText}" ein, z. B. "Dateiname: $TM_FILENAME".</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">Die Beschreibung des Codeausschnitts.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="translated">Eine Liste mit Sprachnamen, für die dieser Codeausschnitt gilt. Beispiel: typescript,javascript.</target>
      </trans-unit>
    </body>
  </file>
