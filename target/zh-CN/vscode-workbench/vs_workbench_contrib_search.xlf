      <trans-unit id="ariaSearchResultsClearStatus">
        <source xml:lang="en">The search results have been cleared</source>
        <target state="translated">搜索结果已清除</target>
      </trans-unit>
      <trans-unit id="searchPathNotFoundError">
        <source xml:lang="en">Search path not found: {0}</source>
        <target state="translated">找不到搜索路径: {0}</target>
      </trans-unit>
