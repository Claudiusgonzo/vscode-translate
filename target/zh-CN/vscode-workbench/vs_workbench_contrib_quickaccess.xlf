  <file original="src/vs/workbench/contrib/quickaccess/browser/quickAccess.contribution" source-language="en" target-language="zh-Hans" datatype="plaintext">
    <body>
      <trans-unit id="helpQuickAccessPlaceholder">
        <source xml:lang="en">Type '{0}' to get help on the actions you can take from here.</source>
        <target state="translated">键入"{0}"以获取有关可在此处执行的操作的帮助。</target>
      </trans-unit>
      <trans-unit id="helpQuickAccess">
        <source xml:lang="en">Show all Quick Access Providers</source>
        <target state="translated">显示所有快速访问提供程序</target>
      </trans-unit>
      <trans-unit id="viewQuickAccessPlaceholder">
        <source xml:lang="en">Type the name of a view, output channel or terminal to open.</source>
        <target state="translated">键入要打开的视图、输出通道或终端的名称。</target>
      </trans-unit>
      <trans-unit id="viewQuickAccess">
        <source xml:lang="en">Open View</source>
        <target state="translated">打开视图</target>
      </trans-unit>
      <trans-unit id="commandsQuickAccessPlaceholder">
        <source xml:lang="en">Type the name of a command to run.</source>
        <target state="translated">键入要运行的命令的名称。</target>
      </trans-unit>
      <trans-unit id="commandsQuickAccess">
        <source xml:lang="en">Show and Run Commands</source>
        <target state="translated">显示并运行命令</target>
      </trans-unit>
      <trans-unit id="miCommandPalette">
        <source xml:lang="en">&amp;&amp;Command Palette...</source>
        <target state="translated">命令面板(&amp;&amp;C)...</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miOpenView">
        <source xml:lang="en">&amp;&amp;Open View...</source>
        <target state="translated">打开视图(&amp;&amp;O)...</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miGotoSymbolInEditor">
        <source xml:lang="en">Go to &amp;&amp;Symbol in Editor...</source>
        <target state="translated">转到编辑器中的 符号(&amp;&amp;S)…</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miGotoLine">
        <source xml:lang="en">Go to &amp;&amp;Line/Column...</source>
        <target state="translated">转到行/列(&amp;&amp;L)...</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="commandPalette">
        <source xml:lang="en">Command Palette...</source>
        <target state="translated">命令面板...</target>
      </trans-unit>
      <trans-unit id="view">
        <source xml:lang="en">View</source>
        <target state="translated">视图</target>
      </trans-unit>
    </body>
  </file>
