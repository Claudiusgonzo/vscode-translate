  <file original="src/vs/workbench/contrib/snippets/browser/snippets.contribution" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="snippetSchema.json.default">
        <source xml:lang="en">Empty snippet</source>
        <target state="translated">Extrait de code vide</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json">
        <source xml:lang="en">User snippet configuration</source>
        <target state="translated">Configuration de l'extrait de code utilisateur</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.prefix">
        <source xml:lang="en">The prefix to used when selecting the snippet in intellisense</source>
        <target state="translated">Préfixe à utiliser durant la sélection de l'extrait de code dans IntelliSense</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.body">
        <source xml:lang="en">The snippet content. Use '$1', '${1:defaultText}' to define cursor positions, use '$0' for the final cursor position. Insert variable values with '${varName}' and '${varName:defaultText}', e.g. 'This is file: $TM_FILENAME'.</source>
        <target state="translated">Contenu de l'extrait de code. Utilisez '$1', '${1:defaultText}' pour définir les positions du curseur, utilisez '$0' pour la position finale du curseur. Insérez des valeurs variables avec '${varName}' et '${varName:defaultText}', par ex. 'This is file: $TM_FILENAME'.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">Description de l'extrait de code.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="translated">Liste des noms de langage auxquels cet extrait de code s'applique, par ex. 'typescript,javascript'.</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/snippets/browser/configureSnippets" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="global.scope">
        <source xml:lang="en">(global)</source>
        <target state="translated">(global)</target>
      </trans-unit>
      <trans-unit id="global.1">
