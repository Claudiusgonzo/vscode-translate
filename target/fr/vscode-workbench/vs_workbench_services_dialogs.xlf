  <file original="src/vs/workbench/services/dialogs/browser/abstractFileDialogService" source-language="en" target-language="fr" datatype="plaintext">
    <body>
      <trans-unit id="saveChangesDetail">
        <source xml:lang="en">Your changes will be lost if you don't save them.</source>
        <target state="translated">Vous perdrez vos modifications, si vous ne les enregistrez pas.</target>
      </trans-unit>
      <trans-unit id="saveChangesMessage">
        <source xml:lang="en">Do you want to save the changes you made to {0}?</source>
        <target state="translated">Voulez-vous enregistrer les modifications apportées à {0} ?</target>
      </trans-unit>
      <trans-unit id="saveChangesMessages">
        <source xml:lang="en">Do you want to save the changes to the following {0} files?</source>
        <target state="translated">Voulez-vous enregistrer les modifications apportées aux {0} fichiers suivants ?</target>
      </trans-unit>
      <trans-unit id="saveAll">
        <source xml:lang="en">&amp;&amp;Save All</source>
        <target state="translated">&amp;&amp;Enregistrer tout</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="save">
        <source xml:lang="en">&amp;&amp;Save</source>
        <target state="translated">Enregi&amp;&amp;strer</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="dontSave">
        <source xml:lang="en">Do&amp;&amp;n't Save</source>
        <target state="translated">&amp;&amp;Ne pas enregistrer</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="cancel">
        <source xml:lang="en">Cancel</source>
        <target state="translated">Annuler</target>
      </trans-unit>
      <trans-unit id="openFileOrFolder.title">
        <source xml:lang="en">Open File Or Folder</source>
        <target state="translated">Ouvrir un fichier ou un dossier</target>
      </trans-unit>
      <trans-unit id="openFile.title">
        <source xml:lang="en">Open File</source>
        <target state="translated">Ouvrir un fichier</target>
      </trans-unit>
      <trans-unit id="openFolder.title">
        <source xml:lang="en">Open Folder</source>
        <target state="translated">Ouvrir le dossier</target>
      </trans-unit>
      <trans-unit id="openWorkspace.title">
        <source xml:lang="en">Open Workspace</source>
        <target state="translated">Ouvrir un espace de travail</target>
      </trans-unit>
      <trans-unit id="filterName.workspace">
        <source xml:lang="en">Workspace</source>
        <target state="translated">Espace de travail</target>
      </trans-unit>
      <trans-unit id="saveFileAs.title">
        <source xml:lang="en">Save As</source>
        <target state="translated">Enregistrer sous</target>
      </trans-unit>
      <trans-unit id="saveAsTitle">
        <source xml:lang="en">Save As</source>
        <target state="translated">Enregistrer sous</target>
      </trans-unit>
      <trans-unit id="allFiles">
        <source xml:lang="en">All Files</source>
        <target state="translated">Tous les fichiers</target>
      </trans-unit>
      <trans-unit id="noExt">
        <source xml:lang="en">No Extension</source>
        <target state="translated">Aucune extension</target>
      </trans-unit>
    </body>
