<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/debug-auto-launch/package" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Node Debug Auto-attach</source>
        <target state="translated">Collegamento automatico per debug di Node</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Helper for auto-attach feature when node-debug extensions are not active.</source>
        <target state="translated">Helper per la funzionalità di collegamento automatico quando le estensioni di debug di Node non sono attive.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.description">
        <source xml:lang="en">Automatically attach node debugger when node.js was launched in debug mode from integrated terminal.</source>
        <target state="translated">Collega automaticamente il debugger di Node quando node.js è stato avviato in modalità debug dal terminale integrato.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.disabled.description">
        <source xml:lang="en">Auto attach is disabled and not shown in status bar.</source>
        <target state="translated">La funzionalità di collegamento automatico è disabilitata e non visualizzata nella barra di stato.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.on.description">
        <source xml:lang="en">Auto attach is active.</source>
        <target state="translated">Il collegamento automatico è attivo.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.off.description">
        <source xml:lang="en">Auto attach is inactive.</source>
        <target state="translated">Il collegamento automatico è inattivo.</target>
      </trans-unit>
      <trans-unit id="toggle.auto.attach">
        <source xml:lang="en">Toggle Auto Attach</source>
        <target state="translated">Attiva/Disattiva collegamento automatico</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/debug-auto-launch/dist/extension" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="status.text.auto.attach.on">
        <source xml:lang="en">Auto Attach: On</source>
        <target state="translated">Collegamento automatico: attivato</target>
      </trans-unit>
      <trans-unit id="status.text.auto.attach.off">
        <source xml:lang="en">Auto Attach: Off</source>
        <target state="translated">Collegamento automatico: disattivato</target>
      </trans-unit>
      <trans-unit id="status.tooltip.auto.attach">
        <source xml:lang="en">Automatically attach to node.js processes in debug mode</source>
        <target state="translated">Connetti automaticamente ai processi di node.js in modalità debug</target>
      </trans-unit>
    </body>
  </file>
</xliff>