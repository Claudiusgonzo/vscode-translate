<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/merge-conflict/package" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Esegui merge del conflitto</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Highlighting and commands for inline merge conflicts.</source>
        <target state="translated">Evidenziazione e comandi per i conflitti di merge inline.</target>
      </trans-unit>
      <trans-unit id="command.category">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Esegui merge del conflitto</target>
      </trans-unit>
      <trans-unit id="command.accept.all-current">
        <source xml:lang="en">Accept All Current</source>
        <target state="translated">Accetta tutte le modifiche correnti</target>
      </trans-unit>
      <trans-unit id="command.accept.all-incoming">
        <source xml:lang="en">Accept All Incoming</source>
        <target state="translated">Accetta tutte le modifiche in ingresso</target>
      </trans-unit>
      <trans-unit id="command.accept.all-both">
        <source xml:lang="en">Accept All Both</source>
        <target state="translated">Accetta tutte in entrambe</target>
      </trans-unit>
      <trans-unit id="command.accept.current">
        <source xml:lang="en">Accept Current</source>
        <target state="translated">Accetta corrente</target>
      </trans-unit>
      <trans-unit id="command.accept.incoming">
        <source xml:lang="en">Accept Incoming</source>
        <target state="translated">Accetta modifiche in ingresso</target>
      </trans-unit>
      <trans-unit id="command.accept.selection">
        <source xml:lang="en">Accept Selection</source>
        <target state="translated">Accetta selezione</target>
      </trans-unit>
      <trans-unit id="command.accept.both">
        <source xml:lang="en">Accept Both</source>
        <target state="translated">Accetta entrambe</target>
      </trans-unit>
      <trans-unit id="command.next">
        <source xml:lang="en">Next Conflict</source>
        <target state="translated">Conflitto successivo</target>
      </trans-unit>
      <trans-unit id="command.previous">
        <source xml:lang="en">Previous Conflict</source>
        <target state="translated">Conflitto precedente</target>
      </trans-unit>
      <trans-unit id="command.compare">
        <source xml:lang="en">Compare Current Conflict</source>
        <target state="translated">Confronta il conflitto corrente</target>
      </trans-unit>
      <trans-unit id="config.title">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Esegui merge del conflitto</target>
      </trans-unit>
      <trans-unit id="config.autoNavigateNextConflictEnabled">
        <source xml:lang="en">Whether to automatically navigate to the next merge conflict after resolving a merge conflict.</source>
        <target state="translated">Indica se passare automaticamente al conflitto successivo dopo la risoluzione di un conflitto di merge.</target>
      </trans-unit>
      <trans-unit id="config.codeLensEnabled">
        <source xml:lang="en">Create a Code Lens for merge conflict blocks within editor.</source>
        <target state="translated">Crea le finestre di CodeLens per i blocchi di conflitti di merge all'interno dell'editor.</target>
      </trans-unit>
      <trans-unit id="config.decoratorsEnabled">
        <source xml:lang="en">Create decorators for merge conflict blocks within editor.</source>
        <target state="translated">Crea elementi Decorator per blocchi di conflitti di merge all'interno dell'editor.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition">
        <source xml:lang="en">Controls where the diff view should be opened when comparing changes in merge conflicts.</source>
        <target state="translated">Controlla dove verrà aperta la visualizzazione differenze quando si confrontano le modifiche nei conflitti di merge.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.current">
        <source xml:lang="en">Open the diff view in the current editor group.</source>
        <target state="translated">Apri la visualizzazione differenze nel gruppo di editor corrente.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.beside">
        <source xml:lang="en">Open the diff view next to the current editor group.</source>
        <target state="translated">Apri la visualizzazione differenze accanto al gruppo di editor corrente.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.below">
        <source xml:lang="en">Open the diff view below the current editor group.</source>
        <target state="translated">Apri la visualizzazione differenze sotto il gruppo di editor corrente.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/codelensProvider" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="acceptCurrentChange">
        <source xml:lang="en">Accept Current Change</source>
        <target state="translated">Accetta modifica corrente</target>
      </trans-unit>
      <trans-unit id="acceptIncomingChange">
        <source xml:lang="en">Accept Incoming Change</source>
        <target state="translated">Accetta modifica in ingresso</target>
      </trans-unit>
      <trans-unit id="acceptBothChanges">
        <source xml:lang="en">Accept Both Changes</source>
        <target state="translated">Accetta entrambe le modifiche</target>
      </trans-unit>
      <trans-unit id="compareChanges">
        <source xml:lang="en">Compare Changes</source>
        <target state="translated">Confronta le modifiche</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/commandHandler" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="cursorNotInConflict">
        <source xml:lang="en">Editor cursor is not within a merge conflict</source>
        <target state="translated">Il cursore dell'editor non si trova all'interno di un conflitto merge</target>
      </trans-unit>
      <trans-unit id="compareChangesTitle">
        <source xml:lang="en">{0}: Current Changes ⟷ Incoming Changes</source>
        <target state="translated">{0}: modifiche correnti ⟷ modifiche in ingresso</target>
      </trans-unit>
      <trans-unit id="cursorOnCommonAncestorsRange">
        <source xml:lang="en">Editor cursor is within the common ancestors block, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Il cursore dell'editor si trova all'interno del blocco di predecessori comuni. Spostarlo nel blocco "corrente" o "in arrivo"</target>
      </trans-unit>
      <trans-unit id="cursorOnSplitterRange">
        <source xml:lang="en">Editor cursor is within the merge conflict splitter, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Il cursore dell'editor si trova all'interno della barra di divisione dei conflitti di merge. Spostarlo nel blocco "corrente" o "in arrivo"</target>
      </trans-unit>
      <trans-unit id="noConflicts">
        <source xml:lang="en">No merge conflicts found in this file</source>
        <target state="translated">Conflitti merge non trovati in questo file</target>
      </trans-unit>
      <trans-unit id="noOtherConflictsInThisFile">
        <source xml:lang="en">No other merge conflicts within this file</source>
        <target state="translated">Nessun altro conflitto merge trovato in questo file</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/mergeDecorator" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="currentChange">
        <source xml:lang="en">(Current Change)</source>
        <target state="translated">(modifica corrente)</target>
      </trans-unit>
      <trans-unit id="incomingChange">
        <source xml:lang="en">(Incoming Change)</source>
        <target state="translated">(modifica in ingresso)</target>
      </trans-unit>
    </body>
  </file>
</xliff>