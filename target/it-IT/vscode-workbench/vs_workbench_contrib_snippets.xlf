  <file original="src/vs/workbench/contrib/snippets/browser/configureSnippets" source-language="en" target-language="it" datatype="plaintext">
    <body>
      <trans-unit id="global.scope">
        <source xml:lang="en">(global)</source>
        <target state="translated">(globale)</target>
      </trans-unit>
      <trans-unit id="global.1">
