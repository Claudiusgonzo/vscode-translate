<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/themes/browser/themes.contribution" source-language="en" target-language="ko" datatype="plaintext">
    <body>
      <trans-unit id="selectTheme.label">
        <source xml:lang="en">Color Theme</source>
        <target state="translated">색 테마</target>
      </trans-unit>
      <trans-unit id="themes.category.light">
        <source xml:lang="en">light themes</source>
        <target state="translated">밝은 테마</target>
      </trans-unit>
      <trans-unit id="themes.category.dark">
        <source xml:lang="en">dark themes</source>
        <target state="translated">어두운 테마</target>
      </trans-unit>
      <trans-unit id="themes.category.hc">
        <source xml:lang="en">high contrast themes</source>
        <target state="translated">고대비 테마</target>
      </trans-unit>
      <trans-unit id="installColorThemes">
        <source xml:lang="en">Install Additional Color Themes...</source>
        <target state="translated">추가 색 테마 설치...</target>
      </trans-unit>
      <trans-unit id="themes.selectTheme">
        <source xml:lang="en">Select Color Theme (Up/Down Keys to Preview)</source>
        <target state="translated">색 테마 선택(미리 보려면 위로/아래로 키 사용)</target>
      </trans-unit>
      <trans-unit id="selectIconTheme.label">
        <source xml:lang="en">File Icon Theme</source>
        <target state="translated">파일 아이콘 테마</target>
      </trans-unit>
      <trans-unit id="noIconThemeLabel">
        <source xml:lang="en">None</source>
        <target state="translated">None</target>
      </trans-unit>
      <trans-unit id="noIconThemeDesc">
        <source xml:lang="en">Disable file icons</source>
        <target state="translated">파일 아이콘 사용 안 함</target>
      </trans-unit>
      <trans-unit id="installIconThemes">
        <source xml:lang="en">Install Additional File Icon Themes...</source>
        <target state="translated">추가 파일 아이콘 테마 설치...</target>
      </trans-unit>
      <trans-unit id="themes.selectIconTheme">
        <source xml:lang="en">Select File Icon Theme</source>
        <target state="translated">파일 아이콘 테마 선택</target>
      </trans-unit>
      <trans-unit id="selectProductIconTheme.label">
        <source xml:lang="en">Product Icon Theme</source>
        <target state="translated">제품 아이콘 테마</target>
      </trans-unit>
      <trans-unit id="defaultProductIconThemeLabel">
        <source xml:lang="en">Default</source>
        <target state="translated">기본값</target>
      </trans-unit>
      <trans-unit id="themes.selectProductIconTheme">
        <source xml:lang="en">Select Product Icon Theme</source>
        <target state="translated">제품 아이콘 테마 선택</target>
      </trans-unit>
      <trans-unit id="generateColorTheme.label">
        <source xml:lang="en">Generate Color Theme From Current Settings</source>
        <target state="translated">현재 설정에서 색 테마 생성</target>
      </trans-unit>
      <trans-unit id="preferences">
        <source xml:lang="en">Preferences</source>
        <target state="translated">기본 설정</target>
      </trans-unit>
      <trans-unit id="developer">
        <source xml:lang="en">Developer</source>
        <target state="translated">개발자</target>
      </trans-unit>
      <trans-unit id="miSelectColorTheme">
        <source xml:lang="en">&amp;&amp;Color Theme</source>
        <target state="translated">색 테마(&amp;&amp;C)</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miSelectIconTheme">
        <source xml:lang="en">File &amp;&amp;Icon Theme</source>
        <target state="translated">파일 아이콘 테마(&amp;&amp;I)</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="themes.selectIconTheme.label">
        <source xml:lang="en">File Icon Theme</source>
        <target state="translated">파일 아이콘 테마</target>
      </trans-unit>
    </body>
  </file>
</xliff>