<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/watermark/browser/watermark" source-language="en" target-language="ko" datatype="plaintext">
    <body>
      <trans-unit id="watermark.showCommands">
        <source xml:lang="en">Show All Commands</source>
        <target state="translated">모든 명령 표시</target>
      </trans-unit>
      <trans-unit id="watermark.quickAccess">
        <source xml:lang="en">Go to File</source>
        <target state="translated">파일로 이동</target>
      </trans-unit>
      <trans-unit id="watermark.openFile">
        <source xml:lang="en">Open File</source>
        <target state="translated">파일 열기</target>
      </trans-unit>
      <trans-unit id="watermark.openFolder">
        <source xml:lang="en">Open Folder</source>
        <target state="translated">폴더 열기</target>
      </trans-unit>
      <trans-unit id="watermark.openFileFolder">
        <source xml:lang="en">Open File or Folder</source>
        <target state="translated">파일 또는 폴더 열기</target>
      </trans-unit>
      <trans-unit id="watermark.openRecent">
        <source xml:lang="en">Open Recent</source>
        <target state="translated">최근 파일 열기</target>
      </trans-unit>
      <trans-unit id="watermark.newUntitledFile">
        <source xml:lang="en">New Untitled File</source>
        <target state="translated">제목이 없는 새 파일</target>
      </trans-unit>
      <trans-unit id="watermark.toggleTerminal">
        <source xml:lang="en">Toggle Terminal</source>
        <target state="translated">터미널 설정/해제</target>
        <note>toggle is a verb here</note>
      </trans-unit>
      <trans-unit id="watermark.findInFiles">
        <source xml:lang="en">Find in Files</source>
        <target state="translated">파일에서 찾기</target>
      </trans-unit>
      <trans-unit id="watermark.startDebugging">
        <source xml:lang="en">Start Debugging</source>
        <target state="translated">디버깅 시작</target>
      </trans-unit>
      <trans-unit id="tips.enabled">
        <source xml:lang="en">When enabled, will show the watermark tips when no editor is open.</source>
        <target state="translated">사용하도록 설정되면 편집기가 열리지 않았을 때 워터마크 팁이 표시됩니다.</target>
      </trans-unit>
    </body>
  </file>
