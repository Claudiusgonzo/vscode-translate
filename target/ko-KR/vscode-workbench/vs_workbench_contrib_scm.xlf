  <file original="src/vs/workbench/contrib/scm/browser/dirtydiffDecorator" source-language="en" target-language="ko" datatype="plaintext">
    <body>
      <trans-unit id="changes">
        <source xml:lang="en">{0} of {1} changes</source>
        <target state="translated">변경 내용 {0}/{1}개</target>
      </trans-unit>
      <trans-unit id="change">
        <source xml:lang="en">{0} of {1} change</source>
        <target state="translated">변경 내용 {0}/{1}개</target>
      </trans-unit>
      <trans-unit id="show previous change">
        <source xml:lang="en">Show Previous Change</source>
        <target state="translated">이전 변경 내용 표시</target>
      </trans-unit>
      <trans-unit id="show next change">
        <source xml:lang="en">Show Next Change</source>
        <target state="translated">다음 변경 내용 표시</target>
      </trans-unit>
      <trans-unit id="miGotoNextChange">
        <source xml:lang="en">Next &amp;&amp;Change</source>
        <target state="translated">다음 변경 내용(&amp;&amp;C)</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="miGotoPreviousChange">
        <source xml:lang="en">Previous &amp;&amp;Change</source>
        <target state="translated">이전 변경 내용(&amp;&amp;C)</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="move to previous change">
        <source xml:lang="en">Move to Previous Change</source>
        <target state="translated">이전 변경 내용으로 이동</target>
      </trans-unit>
      <trans-unit id="move to next change">
        <source xml:lang="en">Move to Next Change</source>
        <target state="translated">다음 변경 내용으로 이동</target>
      </trans-unit>
      <trans-unit id="editorGutterModifiedBackground">
        <source xml:lang="en">Editor gutter background color for lines that are modified.</source>
        <target state="translated">수정된 줄의 편집기 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="editorGutterAddedBackground">
        <source xml:lang="en">Editor gutter background color for lines that are added.</source>
        <target state="translated">추가된 줄의 편집기 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="editorGutterDeletedBackground">
        <source xml:lang="en">Editor gutter background color for lines that are deleted.</source>
        <target state="translated">삭제된 줄의 편집기 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="minimapGutterModifiedBackground">
        <source xml:lang="en">Minimap gutter background color for lines that are modified.</source>
        <target state="translated">수정된 선의 미니맵 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="minimapGutterAddedBackground">
        <source xml:lang="en">Minimap gutter background color for lines that are added.</source>
        <target state="translated">추가된 선의 미니맵 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="minimapGutterDeletedBackground">
        <source xml:lang="en">Minimap gutter background color for lines that are deleted.</source>
        <target state="translated">삭제된 선의 미니맵 여백 배경색입니다.</target>
      </trans-unit>
      <trans-unit id="overviewRulerModifiedForeground">
        <source xml:lang="en">Overview ruler marker color for modified content.</source>
        <target state="translated">수정된 콘텐츠의 개요 눈금자 마커 색입니다.</target>
      </trans-unit>
      <trans-unit id="overviewRulerAddedForeground">
        <source xml:lang="en">Overview ruler marker color for added content.</source>
        <target state="translated">추가된 콘텐츠의 개요 눈금자 마커 색입니다.</target>
      </trans-unit>
      <trans-unit id="overviewRulerDeletedForeground">
        <source xml:lang="en">Overview ruler marker color for deleted content.</source>
        <target state="translated">삭제된 콘텐츠의 개요 눈금자 마커 색입니다.</target>
      </trans-unit>
    </body>
