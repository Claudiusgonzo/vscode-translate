<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="src/vs/workbench/contrib/cli/node/cli.contribution" source-language="en" target-language="ko" datatype="plaintext">
    <body>
      <trans-unit id="install">
        <source xml:lang="en">Install '{0}' command in PATH</source>
        <target state="translated">PATH에 '{0}' 명령 설치</target>
      </trans-unit>
      <trans-unit id="not available">
        <source xml:lang="en">This command is not available</source>
        <target state="translated">이 명령은 사용할 수 없습니다.</target>
      </trans-unit>
      <trans-unit id="successIn">
        <source xml:lang="en">Shell command '{0}' successfully installed in PATH.</source>
        <target state="translated">셸 명령 '{0}'이(가) PATH에 설치되었습니다.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">확인</target>
      </trans-unit>
      <trans-unit id="cancel2">
        <source xml:lang="en">Cancel</source>
        <target state="translated">취소</target>
      </trans-unit>
      <trans-unit id="warnEscalation">
        <source xml:lang="en">Code will now prompt with 'osascript' for Administrator privileges to install the shell command.</source>
        <target state="translated">이제 Code에서 'osascript'를 사용하여 관리자에게 셸 명령을 설치할 권한이 있는지를 묻습니다.</target>
      </trans-unit>
      <trans-unit id="cantCreateBinFolder">
        <source xml:lang="en">Unable to create '/usr/local/bin'.</source>
        <target state="translated">'/usr/local/bin'을 만들 수 없습니다.</target>
      </trans-unit>
      <trans-unit id="aborted">
        <source xml:lang="en">Aborted</source>
        <target state="translated">중단됨</target>
      </trans-unit>
      <trans-unit id="uninstall">
        <source xml:lang="en">Uninstall '{0}' command from PATH</source>
        <target state="translated">PATH에서 '{0}' 명령 제거</target>
      </trans-unit>
      <trans-unit id="successFrom">
        <source xml:lang="en">Shell command '{0}' successfully uninstalled from PATH.</source>
        <target state="translated">셸 명령 '{0}'이(가) PATH에서 제거되었습니다.</target>
      </trans-unit>
      <trans-unit id="warnEscalationUninstall">
        <source xml:lang="en">Code will now prompt with 'osascript' for Administrator privileges to uninstall the shell command.</source>
        <target state="translated">이제 Code에서 'osascript'를 사용하여 셸 명령을 제거할 관리자 권한이 있는지를 묻습니다.</target>
      </trans-unit>
      <trans-unit id="cantUninstall">
        <source xml:lang="en">Unable to uninstall the shell command '{0}'.</source>
        <target state="translated">셸 명령 '{0}'을(를) 제거할 수 없습니다.</target>
      </trans-unit>
      <trans-unit id="shellCommand">
        <source xml:lang="en">Shell Command</source>
        <target state="translated">셸 명령</target>
      </trans-unit>
    </body>
  </file>
</xliff>