<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/json-language-features/package" source-language="en" target-language="zh-Hans" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">JSON Language Features</source>
        <target state="translated">JSON 语言功能</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Provides rich language support for JSON files.</source>
        <target state="translated">为 JSON 文件提供丰富的语言支持</target>
      </trans-unit>
      <trans-unit id="json.schemas.desc">
        <source xml:lang="en">Associate schemas to JSON files in the current project</source>
        <target state="translated">将当前项目中的 JSON 文件与架构关联起来</target>
      </trans-unit>
      <trans-unit id="json.schemas.url.desc">
        <source xml:lang="en">A URL to a schema or a relative path to a schema in the current directory</source>
        <target state="translated">当前目录中指向架构的 URL 或相对路径</target>
      </trans-unit>
      <trans-unit id="json.schemas.fileMatch.desc">
        <source xml:lang="en">An array of file patterns to match against when resolving JSON files to schemas.</source>
        <target state="translated">将 JSON 文件解析到架构时，用于匹配的一组文件模式。</target>
      </trans-unit>
      <trans-unit id="json.schemas.fileMatch.item.desc">
        <source xml:lang="en">A file pattern that can contain '*' to match against when resolving JSON files to schemas.</source>
        <target state="translated">将 JSON 文件解析到架构时，用于匹配的可以包含 "*" 的文件模式。</target>
      </trans-unit>
      <trans-unit id="json.schemas.schema.desc">
        <source xml:lang="en">The schema definition for the given URL. The schema only needs to be provided to avoid accesses to the schema URL.</source>
        <target state="translated">给定 URL 的架构定义。只需提供该架构以避免对架构 URL 的访问。</target>
      </trans-unit>
      <trans-unit id="json.format.enable.desc">
        <source xml:lang="en">Enable/disable default JSON formatter</source>
        <target state="translated">启用或禁用默认 JSON 格式化程序。</target>
      </trans-unit>
      <trans-unit id="json.tracing.desc">
        <source xml:lang="en">Traces the communication between VS Code and the JSON language server.</source>
        <target state="translated">跟踪 VS Code 和 JSON 语言服务器之间的通信。</target>
      </trans-unit>
      <trans-unit id="json.colorDecorators.enable.desc">
        <source xml:lang="en">Enables or disables color decorators</source>
        <target state="translated">启用或禁用颜色修饰器</target>
      </trans-unit>
      <trans-unit id="json.colorDecorators.enable.deprecationMessage">
        <source xml:lang="en">The setting `json.colorDecorators.enable` has been deprecated in favor of `editor.colorDecorators`.</source>
        <target state="translated">已弃用设置 "json.colorDecorators.enable"，请改用 "editor.colorDecorators"。</target>
      </trans-unit>
      <trans-unit id="json.schemaResolutionErrorMessage">
        <source xml:lang="en">Unable to resolve schema.</source>
        <target state="translated">无法解析架构。</target>
      </trans-unit>
      <trans-unit id="json.clickToRetry">
        <source xml:lang="en">Click to retry.</source>
        <target state="translated">单击进行重试。</target>
      </trans-unit>
      <trans-unit id="json.maxItemsComputed.desc">
        <source xml:lang="en">The maximum number of outline symbols and folding regions computed (limited for performance reasons).</source>
        <target state="translated">计算的大纲符号和折叠区域的最大数量(因性能原因而受限)。</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/json-language-features/client\dist/jsonMain" source-language="en" target-language="zh-Hans" datatype="plaintext">
    <body>
      <trans-unit id="json.resolveError">
        <source xml:lang="en">JSON: Schema Resolution Error</source>
        <target state="translated">JSON: 架构解析错误</target>
      </trans-unit>
      <trans-unit id="json.schemaResolutionErrorMessage">
        <source xml:lang="en">Unable to resolve schema.</source>
        <target state="translated">无法解析架构。</target>
      </trans-unit>
      <trans-unit id="json.clickToRetry">
        <source xml:lang="en">Click to retry.</source>
        <target state="translated">单击进行重试。</target>
      </trans-unit>
      <trans-unit id="jsonserver.name">
        <source xml:lang="en">JSON Language Server</source>
        <target state="translated">JSON 语言服务器</target>
      </trans-unit>
    </body>
  </file>
</xliff>