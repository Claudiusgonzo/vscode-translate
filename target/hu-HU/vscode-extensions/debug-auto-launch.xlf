<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/debug-auto-launch/package" source-language="en" target-language="hu" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Node Debug Auto-attach</source>
        <target state="translated">Node hibakeresés automatikus csatolása</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Helper for auto-attach feature when node-debug extensions are not active.</source>
        <target state="translated">Segédcsomag, ami elvégzi a folyamatok automatikus csatolását, ha a node-hibakereső kiegészítők nem aktívak.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.description">
        <source xml:lang="en">Automatically attach node debugger when node.js was launched in debug mode from integrated terminal.</source>
        <target state="translated">Node-hibakereső automatikus csatolása, ha a node.js elindul hibakeresési módban az integrált terminálból.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.disabled.description">
        <source xml:lang="en">Auto attach is disabled and not shown in status bar.</source>
        <target state="translated">Az automatikus csatolás le van tiltva, és nem jelenik meg az állapotsoron.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.on.description">
        <source xml:lang="en">Auto attach is active.</source>
        <target state="translated">Az automatikus csatolás aktív.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.off.description">
        <source xml:lang="en">Auto attach is inactive.</source>
        <target state="translated">Az automatikus csatolás inaktív.</target>
      </trans-unit>
      <trans-unit id="toggle.auto.attach">
        <source xml:lang="en">Toggle Auto Attach</source>
        <target state="translated">Automatikus csatlakozás be- és kikapcsolása</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/debug-auto-launch/dist/extension" source-language="en" target-language="hu" datatype="plaintext">
    <body>
      <trans-unit id="status.text.auto.attach.on">
        <source xml:lang="en">Auto Attach: On</source>
        <target state="translated">Automatikus csatlakozás: bekapcsolva</target>
      </trans-unit>
      <trans-unit id="status.text.auto.attach.off">
        <source xml:lang="en">Auto Attach: Off</source>
        <target state="translated">Automatikus csatlakozás: kikapcsolva</target>
      </trans-unit>
      <trans-unit id="status.tooltip.auto.attach">
        <source xml:lang="en">Automatically attach to node.js processes in debug mode</source>
        <target state="translated">Automatikus csatlakozás node.js-folyamatokhoz hibakeresési módban</target>
      </trans-unit>
    </body>
  </file>
</xliff>