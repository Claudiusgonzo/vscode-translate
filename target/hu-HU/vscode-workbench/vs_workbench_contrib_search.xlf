        <target state="new">Configure effect of double clicking a result in a search editor.</target>
      </trans-unit>
      <trans-unit id="searchSortOrder.default">
        <source xml:lang="en">Results are sorted by folder and file names, in alphabetical order.</source>
        <target state="new">Results are sorted by folder and file names, in alphabetical order.</target>
      </trans-unit>
      <trans-unit id="searchSortOrder.filesOnly">
        <source xml:lang="en">Results are sorted by file names ignoring folder order, in alphabetical order.</source>
        <target state="new">Results are sorted by file names ignoring folder order, in alphabetical order.</target>
      </trans-unit>
      <trans-unit id="searchSortOrder.type">
