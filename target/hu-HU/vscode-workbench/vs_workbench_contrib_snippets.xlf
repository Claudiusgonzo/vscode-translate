      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">A kódtöredék leírása.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="new">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</target>
      </trans-unit>
