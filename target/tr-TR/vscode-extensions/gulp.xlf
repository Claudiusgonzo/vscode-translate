<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/gulp/package" source-language="en" target-language="tr" datatype="plaintext">
    <body>
      <trans-unit id="description">
        <source xml:lang="en">Extension to add Gulp capabilities to VSCode.</source>
        <target state="translated">VSCode'a Gulp yetenekleri kazandıracak eklenti.</target>
      </trans-unit>
      <trans-unit id="displayName">
        <source xml:lang="en">Gulp support for VSCode</source>
        <target state="translated">VSCode için Gulp desteği</target>
      </trans-unit>
      <trans-unit id="config.gulp.autoDetect">
        <source xml:lang="en">Controls whether auto detection of Gulp tasks is on or off. Default is on.</source>
        <target state="translated">Gulp görevlerinin otomatik olarak algılanıp algılanmayacağını denetler. Varsayılan olarak açıktır.</target>
      </trans-unit>
      <trans-unit id="gulp.taskDefinition.type.description">
        <source xml:lang="en">The Gulp task to customize.</source>
        <target state="translated">Özelleştirilecek Gulp görevi.</target>
      </trans-unit>
      <trans-unit id="gulp.taskDefinition.file.description">
        <source xml:lang="en">The Gulp file that provides the task. Can be omitted.</source>
        <target state="translated">Görevi sağlayan Gulp dosyası. Atlanabilir.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/gulp/dist/main" source-language="en" target-language="tr" datatype="plaintext">
    <body>
      <trans-unit id="gulpTaskDetectError">
        <source xml:lang="en">Problem finding gulp tasks. See the output for more information.</source>
        <target state="new">Problem finding gulp tasks. See the output for more information.</target>
      </trans-unit>
      <trans-unit id="gulpShowOutput">
        <source xml:lang="en">Go to output</source>
        <target state="new">Go to output</target>
      </trans-unit>
      <trans-unit id="execFailed">
        <source xml:lang="en">Auto detecting gulp for folder {0} failed with error: {1}</source>
        <target state="translated">{0} klasörü için gulp otomatik tespiti hata ile sonuçlandı: {1}</target>
      </trans-unit>
    </body>
  </file>
