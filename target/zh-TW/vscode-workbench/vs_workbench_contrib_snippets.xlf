  <file original="src/vs/workbench/contrib/snippets/browser/snippets.contribution" source-language="en" target-language="zh-Hant" datatype="plaintext">
    <body>
      <trans-unit id="snippetSchema.json.default">
        <source xml:lang="en">Empty snippet</source>
        <target state="translated">空白程式碼片段</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json">
        <source xml:lang="en">User snippet configuration</source>
        <target state="translated">使用者程式碼片段組態</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.prefix">
        <source xml:lang="en">The prefix to used when selecting the snippet in intellisense</source>
        <target state="translated">在 Intellisense 中選取程式碼片段時要使用的前置詞</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.body">
        <source xml:lang="en">The snippet content. Use '$1', '${1:defaultText}' to define cursor positions, use '$0' for the final cursor position. Insert variable values with '${varName}' and '${varName:defaultText}', e.g. 'This is file: $TM_FILENAME'.</source>
        <target state="translated">程式碼片段內容。請使用 '$1', '${1:defaultText}' 來定義資料指標位置，使用 '$0' 代表最終資料指標位置。請以 '${varName}' 和 '${varName:defaultText}' 插入變數值，例如 'This is file: $TM_FILENAME'。</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">程式碼片段描述。</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="translated">此代碼片段應用的編程語言清單，例如 "typescript,javascript"。</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/snippets/browser/configureSnippets" source-language="en" target-language="zh-Hant" datatype="plaintext">
    <body>
      <trans-unit id="global.scope">
        <source xml:lang="en">(global)</source>
        <target state="translated">(全域)</target>
      </trans-unit>
      <trans-unit id="global.1">
        <source xml:lang="en">({0})</source>
        <target state="translated">({0})</target>
      </trans-unit>
      <trans-unit id="name">
