  <file original="src/vs/workbench/contrib/format/browser/formatActionsNone" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document</source>
        <target state="translated">Formatar Documento</target>
      </trans-unit>
      <trans-unit id="no.provider">
        <source xml:lang="en">There is no formatter for '{0}' files installed.</source>
        <target state="translated">Não há formatador para arquivos '{0}' instalado.</target>
      </trans-unit>
      <trans-unit id="install.formatter">
        <source xml:lang="en">Install Formatter...</source>
        <target state="translated">Instalar o formatador...</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/format/browser/formatActionsMultiple" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="nullFormatterDescription">
        <source xml:lang="en">None</source>
        <target state="translated">NENHUM</target>
      </trans-unit>
      <trans-unit id="miss">
        <source xml:lang="en">Extension '{0}' cannot format '{1}'</source>
        <target state="translated">A extensão '{0}' não pode formatar '{1}'</target>
      </trans-unit>
      <trans-unit id="config.needed">
        <source xml:lang="en">There are multiple formatters for '{0}' files. Select a default formatter to continue.</source>
        <target state="translated">Existem vários formatadores para arquivos '{0}'. Selecione um formatador padrão para continuar.</target>
      </trans-unit>
      <trans-unit id="config.bad">
        <source xml:lang="en">Extension '{0}' is configured as formatter but not available. Select a different default formatter to continue.</source>
        <target state="translated">Extensão '{0}' é configurada como formatador mas não está disponível. Selecione um formatador padrão diferente para continuar.</target>
      </trans-unit>
      <trans-unit id="do.config">
        <source xml:lang="en">Configure...</source>
        <target state="translated">Configurar...</target>
      </trans-unit>
      <trans-unit id="select">
        <source xml:lang="en">Select a default formatter for '{0}' files</source>
        <target state="translated">Selecione um formatador padrão para arquivos '{0}'</target>
      </trans-unit>
      <trans-unit id="formatter.default">
        <source xml:lang="en">Defines a default formatter which takes precedence over all other formatter settings. Must be the identifier of an extension contributing a formatter.</source>
        <target state="translated">Define um formatador padrão que tem precedência sobre o formatador de todas as outras configurações. Deve ser o identificador de uma extensão que contribui com um formatador.</target>
      </trans-unit>
      <trans-unit id="def">
        <source xml:lang="en">(default)</source>
        <target state="translated">(Padrão)</target>
      </trans-unit>
      <trans-unit id="config">
        <source xml:lang="en">Configure Default Formatter...</source>
        <target state="translated">Configurar o Formatador Padrão...</target>
      </trans-unit>
      <trans-unit id="format.placeHolder">
        <source xml:lang="en">Select a formatter</source>
        <target state="translated">Selecione um formatador</target>
      </trans-unit>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document With...</source>
        <target state="translated">Formatar documento com...</target>
      </trans-unit>
      <trans-unit id="formatSelection.label.multiple">
        <source xml:lang="en">Format Selection With...</source>
        <target state="translated">Formatar seleção com...</target>
      </trans-unit>
    </body>
  </file>
