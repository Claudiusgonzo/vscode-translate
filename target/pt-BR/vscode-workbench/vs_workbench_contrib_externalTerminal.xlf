        <target state="translated">Script '{0}' falhou com código de saída {1}</target>
      </trans-unit>
      <trans-unit id="mac.terminal.type.not.supported">
        <source xml:lang="en">'{0}' not supported</source>
        <target state="translated">'{0}' não suportado</target>
      </trans-unit>
