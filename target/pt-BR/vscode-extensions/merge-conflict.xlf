<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/merge-conflict/package" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Mesclar conflitos</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Highlighting and commands for inline merge conflicts.</source>
        <target state="translated">Realce e comandos para mesclagem de conflitos inline.</target>
      </trans-unit>
      <trans-unit id="command.category">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Mesclar conflitos</target>
      </trans-unit>
      <trans-unit id="command.accept.all-current">
        <source xml:lang="en">Accept All Current</source>
        <target state="translated">Aceitar Todos os Atuais</target>
      </trans-unit>
      <trans-unit id="command.accept.all-incoming">
        <source xml:lang="en">Accept All Incoming</source>
        <target state="translated">Aceitar todas entradas</target>
      </trans-unit>
      <trans-unit id="command.accept.all-both">
        <source xml:lang="en">Accept All Both</source>
        <target state="translated">Aceitar todas as duas</target>
      </trans-unit>
      <trans-unit id="command.accept.current">
        <source xml:lang="en">Accept Current</source>
        <target state="translated">Aceitar a corrente</target>
      </trans-unit>
      <trans-unit id="command.accept.incoming">
        <source xml:lang="en">Accept Incoming</source>
        <target state="translated">Aceitar entrada</target>
      </trans-unit>
      <trans-unit id="command.accept.selection">
        <source xml:lang="en">Accept Selection</source>
        <target state="translated">Aceitar a seleção</target>
      </trans-unit>
      <trans-unit id="command.accept.both">
        <source xml:lang="en">Accept Both</source>
        <target state="translated">Aceitar Ambos</target>
      </trans-unit>
      <trans-unit id="command.next">
        <source xml:lang="en">Next Conflict</source>
        <target state="translated">Próximo conflito</target>
      </trans-unit>
      <trans-unit id="command.previous">
        <source xml:lang="en">Previous Conflict</source>
        <target state="translated">Conflito anterior</target>
      </trans-unit>
      <trans-unit id="command.compare">
        <source xml:lang="en">Compare Current Conflict</source>
        <target state="translated">Comparar o conflito atual</target>
      </trans-unit>
      <trans-unit id="config.title">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Mesclar conflitos</target>
      </trans-unit>
      <trans-unit id="config.autoNavigateNextConflictEnabled">
        <source xml:lang="en">Whether to automatically navigate to the next merge conflict after resolving a merge conflict.</source>
        <target state="translated">Se deseja navegar automaticamente para o próximo conflito de merge após resolver um conflito de merge.</target>
      </trans-unit>
      <trans-unit id="config.codeLensEnabled">
        <source xml:lang="en">Create a Code Lens for merge conflict blocks within editor.</source>
        <target state="translated">Crie um CodeLens para blocos de conflitos de mesclagem dentro do editor.</target>
      </trans-unit>
      <trans-unit id="config.decoratorsEnabled">
        <source xml:lang="en">Create decorators for merge conflict blocks within editor.</source>
        <target state="translated">Crie decoradores para blocos de conflitos de mesclagem dentro do editor.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition">
        <source xml:lang="en">Controls where the diff view should be opened when comparing changes in merge conflicts.</source>
        <target state="translated">Controla onde a exibição de comparação deve ser aberta ao comparar alterações em conflitos de mesclagem.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.current">
        <source xml:lang="en">Open the diff view in the current editor group.</source>
        <target state="translated">Abra a exibição de comparação no grupo do editor atual.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.beside">
        <source xml:lang="en">Open the diff view next to the current editor group.</source>
        <target state="translated">Abra a exibição de comparação ao lado do grupo de edição atual.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.below">
        <source xml:lang="en">Open the diff view below the current editor group.</source>
        <target state="translated">Abra o modo de exibição de comparação abaixo do grupo de edição atual.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/codelensProvider" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="acceptCurrentChange">
        <source xml:lang="en">Accept Current Change</source>
        <target state="translated">Aceitar a mudança atual</target>
      </trans-unit>
      <trans-unit id="acceptIncomingChange">
        <source xml:lang="en">Accept Incoming Change</source>
        <target state="translated">Aceitar a mudança de entrada</target>
      </trans-unit>
      <trans-unit id="acceptBothChanges">
        <source xml:lang="en">Accept Both Changes</source>
        <target state="translated">Aceitar as duas alterações</target>
      </trans-unit>
      <trans-unit id="compareChanges">
        <source xml:lang="en">Compare Changes</source>
        <target state="translated">Comparar as mudanças</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/commandHandler" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="cursorNotInConflict">
        <source xml:lang="en">Editor cursor is not within a merge conflict</source>
        <target state="translated">Cursor do editor não está dentro de um conflito de mesclagem</target>
      </trans-unit>
      <trans-unit id="compareChangesTitle">
        <source xml:lang="en">{0}: Current Changes ⟷ Incoming Changes</source>
        <target state="translated">{0}: Alterações Atuais ⟷ Alterações de Entrada</target>
      </trans-unit>
      <trans-unit id="cursorOnCommonAncestorsRange">
        <source xml:lang="en">Editor cursor is within the common ancestors block, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Cursor do editor está dentro do bloco comum de ancestrais, favor mover para o bloco "atual" ou "entrada"</target>
      </trans-unit>
      <trans-unit id="cursorOnSplitterRange">
        <source xml:lang="en">Editor cursor is within the merge conflict splitter, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Cursor do editor está dentro do separador de conflitos de mesclagem, por favor mova-o para o bloco "atual" ou "entrada"</target>
      </trans-unit>
      <trans-unit id="noConflicts">
        <source xml:lang="en">No merge conflicts found in this file</source>
        <target state="translated">Nenhum conflito de mesclagem encontrado neste arquivo</target>
      </trans-unit>
      <trans-unit id="noOtherConflictsInThisFile">
        <source xml:lang="en">No other merge conflicts within this file</source>
        <target state="translated">Não há outros conflitos de mesclagem dentro desse arquivo</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/mergeDecorator" source-language="en" target-language="pt-BR" datatype="plaintext">
    <body>
      <trans-unit id="currentChange">
        <source xml:lang="en">(Current Change)</source>
        <target state="translated">(Mudança Atual)</target>
      </trans-unit>
      <trans-unit id="incomingChange">
        <source xml:lang="en">(Incoming Change)</source>
        <target state="translated">(Próxima Mudança)</target>
      </trans-unit>
    </body>
  </file>
</xliff>