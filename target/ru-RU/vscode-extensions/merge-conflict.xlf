<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/merge-conflict/package" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Объединить конфликт</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Highlighting and commands for inline merge conflicts.</source>
        <target state="translated">Выделение и команды для внутренних конфликтов слияния.</target>
      </trans-unit>
      <trans-unit id="command.category">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Объединить конфликт</target>
      </trans-unit>
      <trans-unit id="command.accept.all-current">
        <source xml:lang="en">Accept All Current</source>
        <target state="translated">Принять все текущие</target>
      </trans-unit>
      <trans-unit id="command.accept.all-incoming">
        <source xml:lang="en">Accept All Incoming</source>
        <target state="translated">Принять все входящие</target>
      </trans-unit>
      <trans-unit id="command.accept.all-both">
        <source xml:lang="en">Accept All Both</source>
        <target state="translated">Принять все входящие и текущие</target>
      </trans-unit>
      <trans-unit id="command.accept.current">
        <source xml:lang="en">Accept Current</source>
        <target state="translated">Принять текущее</target>
      </trans-unit>
      <trans-unit id="command.accept.incoming">
        <source xml:lang="en">Accept Incoming</source>
        <target state="translated">Принять входящее</target>
      </trans-unit>
      <trans-unit id="command.accept.selection">
        <source xml:lang="en">Accept Selection</source>
        <target state="translated">Принять выделенное</target>
      </trans-unit>
      <trans-unit id="command.accept.both">
        <source xml:lang="en">Accept Both</source>
        <target state="translated">Принять оба</target>
      </trans-unit>
      <trans-unit id="command.next">
        <source xml:lang="en">Next Conflict</source>
        <target state="translated">Следующий конфликт</target>
      </trans-unit>
      <trans-unit id="command.previous">
        <source xml:lang="en">Previous Conflict</source>
        <target state="translated">Предыдущий конфликт</target>
      </trans-unit>
      <trans-unit id="command.compare">
        <source xml:lang="en">Compare Current Conflict</source>
        <target state="translated">Сравнить текущий конфликт</target>
      </trans-unit>
      <trans-unit id="config.title">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Объединить конфликт</target>
      </trans-unit>
      <trans-unit id="config.autoNavigateNextConflictEnabled">
        <source xml:lang="en">Whether to automatically navigate to the next merge conflict after resolving a merge conflict.</source>
        <target state="translated">Следует ли автоматически перейти к следующему конфликту слияния после устранения конфликта слияния.</target>
      </trans-unit>
      <trans-unit id="config.codeLensEnabled">
        <source xml:lang="en">Create a Code Lens for merge conflict blocks within editor.</source>
        <target state="translated">Создать Code Lens для блоков объединения конфликтов в редакторе.</target>
      </trans-unit>
      <trans-unit id="config.decoratorsEnabled">
        <source xml:lang="en">Create decorators for merge conflict blocks within editor.</source>
        <target state="translated">Создать декораторы для блоков объединения конфликтов в редакторе.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition">
        <source xml:lang="en">Controls where the diff view should be opened when comparing changes in merge conflicts.</source>
        <target state="translated">Определяет, где следует открыть представление инструмента сравнения при сравнении изменений в конфликтах слияния.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.current">
        <source xml:lang="en">Open the diff view in the current editor group.</source>
        <target state="translated">Откройте представление инструмента сравнения в текущей группе редакторов.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.beside">
        <source xml:lang="en">Open the diff view next to the current editor group.</source>
        <target state="translated">Откройте представление инструмента сравнения рядом с текущей группой редакторов.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.below">
        <source xml:lang="en">Open the diff view below the current editor group.</source>
        <target state="translated">Откройте представление инструмента сравнения под текущей группой редакторов.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/codelensProvider" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="acceptCurrentChange">
        <source xml:lang="en">Accept Current Change</source>
        <target state="translated">Принять текущее изменение</target>
      </trans-unit>
      <trans-unit id="acceptIncomingChange">
        <source xml:lang="en">Accept Incoming Change</source>
        <target state="translated">Принять входящее изменение</target>
      </trans-unit>
      <trans-unit id="acceptBothChanges">
        <source xml:lang="en">Accept Both Changes</source>
        <target state="translated">Принять оба изменения</target>
      </trans-unit>
      <trans-unit id="compareChanges">
        <source xml:lang="en">Compare Changes</source>
        <target state="translated">Сравнить изменения</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/commandHandler" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="cursorNotInConflict">
        <source xml:lang="en">Editor cursor is not within a merge conflict</source>
        <target state="translated">Курсор не находится на конфликте объединения</target>
      </trans-unit>
      <trans-unit id="compareChangesTitle">
        <source xml:lang="en">{0}: Current Changes ⟷ Incoming Changes</source>
        <target state="translated">{0}: текущие изменения ⟷ входящие изменения</target>
      </trans-unit>
      <trans-unit id="cursorOnCommonAncestorsRange">
        <source xml:lang="en">Editor cursor is within the common ancestors block, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Курсор редактора находится в блоке общих предков. Переместите его в блок "Текущее" или "Входящее"</target>
      </trans-unit>
      <trans-unit id="cursorOnSplitterRange">
        <source xml:lang="en">Editor cursor is within the merge conflict splitter, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Курсор редактора находится на разделителе блока объединения конфликтов. Переместите его в блок "Текущее" или "Входящее"</target>
      </trans-unit>
      <trans-unit id="noConflicts">
        <source xml:lang="en">No merge conflicts found in this file</source>
        <target state="translated">Конфликтов объединения в этом файле не обнаружено</target>
      </trans-unit>
      <trans-unit id="noOtherConflictsInThisFile">
        <source xml:lang="en">No other merge conflicts within this file</source>
        <target state="translated">Других конфликтов объединения в этом файле не обнаружено</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/mergeDecorator" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="currentChange">
        <source xml:lang="en">(Current Change)</source>
        <target state="translated">(текущее изменение)</target>
      </trans-unit>
      <trans-unit id="incomingChange">
        <source xml:lang="en">(Incoming Change)</source>
        <target state="translated">(входящее изменение)</target>
      </trans-unit>
    </body>
  </file>
</xliff>