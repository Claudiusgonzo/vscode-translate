    <body>
      <trans-unit id="sync preferences">
        <source xml:lang="en">Preferences Sync</source>
        <target state="translated">Синхронизация настроек</target>
      </trans-unit>
      <trans-unit id="remote title">
        <source xml:lang="en">Remote Backup</source>
