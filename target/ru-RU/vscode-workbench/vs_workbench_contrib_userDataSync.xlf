        <target state="translated">Показать удаленную резервную копию</target>
      </trans-unit>
      <trans-unit id="workbench.action.showSyncLocalBackup">
        <source xml:lang="en">Show Local Backup</source>
        <target state="translated">Показать локальную резервную копию</target>
