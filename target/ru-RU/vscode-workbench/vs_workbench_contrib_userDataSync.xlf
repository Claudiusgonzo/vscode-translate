  <file original="src/vs/workbench/contrib/userDataSync/browser/userDataSync" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="settings">
        <source xml:lang="en">Settings</source>
        <target state="translated">Параметры</target>
      </trans-unit>
      <trans-unit id="keybindings">
        <source xml:lang="en">Keybindings</source>
        <target state="translated">Настраиваемые сочетания клавиш</target>
      </trans-unit>
      <trans-unit id="extensions">
        <source xml:lang="en">Extensions</source>
        <target state="translated">Расширения</target>
      </trans-unit>
      <trans-unit id="ui state label">
        <source xml:lang="en">UI State</source>
        <target state="translated">Состояние пользовательского интерфейса</target>
      </trans-unit>
      <trans-unit id="sync is on with syncing">
        <source xml:lang="en">{0} (syncing)</source>
        <target state="translated">{0} (синхронизация)</target>
      </trans-unit>
      <trans-unit id="sync is on with time">
        <source xml:lang="en">{0} (synced {1})</source>
        <target state="translated">{0} (синхронизировано: {1})</target>
      </trans-unit>
      <trans-unit id="turn on sync with category">
        <source xml:lang="en">Sync: Turn on Sync</source>
        <target state="translated">Синхронизация: включение синхронизации</target>
      </trans-unit>
      <trans-unit id="sign in">
        <source xml:lang="en">Sync: Sign in to sync</source>
        <target state="translated">Синхронизация: вход для синхронизации</target>
      </trans-unit>
      <trans-unit id="stop sync">
        <source xml:lang="en">Sync: Turn off Sync</source>
        <target state="translated">Синхронизация: отключить синхронизацию</target>
      </trans-unit>
      <trans-unit id="showConflicts">
        <source xml:lang="en">Sync: Show Settings Conflicts</source>
        <target state="translated">Синхронизация: отображение конфликтов параметров</target>
      </trans-unit>
      <trans-unit id="showKeybindingsConflicts">
        <source xml:lang="en">Sync: Show Keybindings Conflicts</source>
        <target state="translated">Синхронизация: отображение конфликтов настраиваемых сочетаний клавиш</target>
      </trans-unit>
      <trans-unit id="configure sync">
        <source xml:lang="en">Sync: Configure</source>
        <target state="translated">Синхронизация: настроить</target>
      </trans-unit>
      <trans-unit id="show sync log">
        <source xml:lang="en">Sync: Show Log</source>
        <target state="translated">Синхронизация: отображение журнала</target>
      </trans-unit>
      <trans-unit id="sync settings">
        <source xml:lang="en">Sync: Settings</source>
        <target state="translated">Синхронизация: параметры</target>
      </trans-unit>
      <trans-unit id="conflicts detected">
        <source xml:lang="en">Unable to sync due to conflicts in {0}. Please resolve them to continue.</source>
        <target state="translated">Не удается синхронизировать из-за конфликтов в {0}. Устраните их, чтобы продолжить.</target>
      </trans-unit>
      <trans-unit id="accept remote">
        <source xml:lang="en">Accept Remote</source>
        <target state="translated">Принять удаленный</target>
      </trans-unit>
      <trans-unit id="accept local">
        <source xml:lang="en">Accept Local</source>
        <target state="translated">Принять локальный</target>
      </trans-unit>
      <trans-unit id="show conflicts">
        <source xml:lang="en">Show Conflicts</source>
        <target state="translated">Показать конфиликты</target>
      </trans-unit>
      <trans-unit id="sign in message">
        <source xml:lang="en">Please sign in with your {0} account to continue sync</source>
        <target state="translated">Войдите с помощью своей учетной записи {0}, чтобы продолжить синхронизацию</target>
      </trans-unit>
      <trans-unit id="Sign in">
        <source xml:lang="en">Sign in</source>
        <target state="translated">Войти</target>
      </trans-unit>
      <trans-unit id="turned off">
        <source xml:lang="en">Sync was turned off from another device.</source>
        <target state="translated">Синхронизация была отключена с другого устройства.</target>
      </trans-unit>
      <trans-unit id="turn on sync">
        <source xml:lang="en">Turn on Sync</source>
        <target state="translated">Включить синхронизацию</target>
      </trans-unit>
      <trans-unit id="too large">
        <source xml:lang="en">Disabled syncing {0} because size of the {1} file to sync is larger than {2}. Please open the file and reduce the size and enable sync</source>
        <target state="translated">Синхронизация {0} отключена, так как размер файла {1} для синхронизации больше {2}. Откройте файл, уменьшите размер и включите синхронизацию.</target>
      </trans-unit>
      <trans-unit id="open file">
        <source xml:lang="en">Open {0} File</source>
        <target state="translated">Открыть файл {0}</target>
      </trans-unit>
      <trans-unit id="error incompatible">
        <source xml:lang="en">Turned off sync because local data is incompatible with the data in the cloud. Please update {0} and turn on sync to continue syncing.</source>
        <target state="translated">Синхронизация отключена, так как локальные данные несовместимы с данными в облаке. Чтобы продолжить синхронизацию, обновите {0} и включите синхронизацию.</target>
      </trans-unit>
      <trans-unit id="errorInvalidConfiguration">
        <source xml:lang="en">Unable to sync {0} because there are some errors/warnings in the file. Please open the file to correct errors/warnings in it.</source>
        <target state="translated">Не удалось синхронизировать {0}, так как в файле есть ошибки/предупреждения. Откройте файл для исправления содержащихся в нем ошибок/предупреждений.</target>
      </trans-unit>
      <trans-unit id="sign in to sync">
        <source xml:lang="en">Sign in to Sync</source>
        <target state="translated">Войдите в систему для синхронизации.</target>
      </trans-unit>
      <trans-unit id="has conflicts">
        <source xml:lang="en">Sync: Conflicts Detected</source>
        <target state="translated">Синхронизация: Обнаружены конфликты</target>
      </trans-unit>
      <trans-unit id="sync preview message">
        <source xml:lang="en">Synchronizing your preferences is a preview feature, please read the documentation before turning it on.</source>
        <target state="translated">Синхронизация ваших настроек является предварительной версией функции, перед ее включением изучите документацию.</target>
      </trans-unit>
      <trans-unit id="open doc">
        <source xml:lang="en">Open Documentation</source>
        <target state="translated">Открыть документацию</target>
      </trans-unit>
      <trans-unit id="cancel">
        <source xml:lang="en">Cancel</source>
        <target state="translated">Отмена</target>
      </trans-unit>
      <trans-unit id="turn on title">
        <source xml:lang="en">Sync: Turn On</source>
        <target state="translated">Синхронизация: включение</target>
      </trans-unit>
      <trans-unit id="turn on">
        <source xml:lang="en">Turn On</source>
        <target state="translated">Включить</target>
      </trans-unit>
      <trans-unit id="sign in and turn on sync detail">
        <source xml:lang="en">Sign in with your {0} account to synchronize your data across devices.</source>
        <target state="translated">Выполните вход с помощью своей учетной записи {0} для синхронизации данных на устройствах.</target>
      </trans-unit>
      <trans-unit id="sign in and turn on sync">
        <source xml:lang="en">Sign in &amp; Turn on</source>
        <target state="translated">Войти и включить</target>
      </trans-unit>
      <trans-unit id="configure sync placeholder">
        <source xml:lang="en">Choose what to sync</source>
        <target state="translated">Выберите компоненты для синхронизации</target>
      </trans-unit>
      <trans-unit id="pick account">
        <source xml:lang="en">{0}: Pick an account</source>
        <target state="translated">{0}: Выберите учетную запись</target>
      </trans-unit>
      <trans-unit id="choose account placeholder">
        <source xml:lang="en">Pick an account for syncing</source>
        <target state="translated">Выберите учетную запись для синхронизации</target>
      </trans-unit>
      <trans-unit id="existing">
        <source xml:lang="en">{0}</source>
        <target state="translated">{0}</target>
      </trans-unit>
      <trans-unit id="signed in">
        <source xml:lang="en">Signed in</source>
        <target state="translated">Выполнен вход</target>
      </trans-unit>
      <trans-unit id="choose another">
        <source xml:lang="en">Use another account</source>
        <target state="translated">Используйте другую учетную запись</target>
      </trans-unit>
      <trans-unit id="sync turned on">
        <source xml:lang="en">Sync will happen automatically from now on.</source>
        <target state="translated">Теперь синхронизация будет происходить автоматически.</target>
      </trans-unit>
      <trans-unit id="ui state description">
        <source xml:lang="en">only 'Display Language' for now</source>
        <target state="translated">только "Язык интерфейса" на данный момент</target>
      </trans-unit>
      <trans-unit id="firs time sync">
        <source xml:lang="en">Sync</source>
        <target state="translated">Синхронизация</target>
      </trans-unit>
      <trans-unit id="merge">
        <source xml:lang="en">Merge</source>
        <target state="translated">Слияние</target>
      </trans-unit>
      <trans-unit id="replace">
        <source xml:lang="en">Replace Local</source>
        <target state="translated">Заменить локальный</target>
      </trans-unit>
      <trans-unit id="first time sync detail">
        <source xml:lang="en">It looks like this is the first time sync is set up.
Would you like to merge or replace with the data from the cloud?</source>
        <target state="translated">Похоже, что синхронизация настроена впервые.
Вы хотите выполнить объединение или замену, используя данные из облака?</target>
      </trans-unit>
      <trans-unit id="turn off sync confirmation">
        <source xml:lang="en">Turn off Sync</source>
        <target state="translated">Отключить синхронизацию</target>
      </trans-unit>
      <trans-unit id="turn off sync detail">
        <source xml:lang="en">Your settings, keybindings, extensions and UI State will no longer be synced.</source>
        <target state="translated">Ваши параметры, настраиваемые сочетания клавиш, расширения и состояние пользовательского интерфейса больше не будут синхронизироваться.</target>
      </trans-unit>
      <trans-unit id="turn off">
        <source xml:lang="en">Turn Off</source>
        <target state="translated">Отключить</target>
      </trans-unit>
      <trans-unit id="turn off sync everywhere">
        <source xml:lang="en">Turn off sync on all your devices and clear the data from the cloud.</source>
        <target state="translated">Отключение синхронизации всех устройств и очистка данных из облака.</target>
      </trans-unit>
      <trans-unit id="loginFailed">
        <source xml:lang="en">Logging in failed: {0}</source>
        <target state="translated">Ошибка входа: {0}</target>
      </trans-unit>
      <trans-unit id="settings conflicts preview">
        <source xml:lang="en">Settings Conflicts (Remote ↔ Local)</source>
        <target state="translated">Конфликты параметров (удаленные ↔ локальные)</target>
      </trans-unit>
      <trans-unit id="keybindings conflicts preview">
        <source xml:lang="en">Keybindings Conflicts (Remote ↔ Local)</source>
        <target state="translated">Конфликты настраиваемых сочетаний клавиш (удаленные ↔ локальные)</target>
      </trans-unit>
      <trans-unit id="turn on failed">
        <source xml:lang="en">Error while starting Sync: {0}</source>
        <target state="translated">Ошибка при запуске синхронизации: {0}</target>
      </trans-unit>
      <trans-unit id="global activity turn on sync">
        <source xml:lang="en">Turn on Sync...</source>
        <target state="translated">Включить синхронизацию...</target>
      </trans-unit>
      <trans-unit id="resolveConflicts_global">
        <source xml:lang="en">Sync: Show Settings Conflicts (1)</source>
        <target state="translated">Синхронизация: отображение конфликтов параметров (1)</target>
      </trans-unit>
      <trans-unit id="resolveKeybindingsConflicts_global">
        <source xml:lang="en">Sync: Show Keybindings Conflicts (1)</source>
        <target state="translated">Синхронизация: отображение конфликтов настраиваемых сочетаний клавиш (1)</target>
      </trans-unit>
      <trans-unit id="sync is on">
        <source xml:lang="en">Sync is on</source>
        <target state="translated">Синхронизация включена</target>
      </trans-unit>
      <trans-unit id="turn off failed">
        <source xml:lang="en">Error while turning off sync: {0}</source>
        <target state="translated">Ошибка при отключении синхронизации: {0}</target>
      </trans-unit>
      <trans-unit id="Sync accept remote">
        <source xml:lang="en">Sync: {0}</source>
        <target state="translated">Синхронизация: {0}</target>
      </trans-unit>
      <trans-unit id="Sync accept local">
        <source xml:lang="en">Sync: {0}</source>
        <target state="translated">Синхронизация: {0}</target>
      </trans-unit>
      <trans-unit id="confirm replace and overwrite local">
        <source xml:lang="en">Would you like to accept Remote {0} and replace Local {1}?</source>
        <target state="translated">Вы хотите принять удаленный {0} и заменить локальный {1}?</target>
      </trans-unit>
      <trans-unit id="confirm replace and overwrite remote">
        <source xml:lang="en">Would you like to accept Local {0} and replace Remote {1}?</source>
        <target state="translated">Вы хотите принять локальный {0} и заменить удаленный {1}?</target>
      </trans-unit>
      <trans-unit id="update conflicts">
        <source xml:lang="en">Could not resolve conflicts as there is new local version available. Please try again.</source>
        <target state="translated">Не удалось разрешить конфликты, так как доступна новая локальная версия. Повторите попытку.</target>
      </trans-unit>
    </body>
