  <file original="src/vs/workbench/contrib/snippets/browser/snippets.contribution" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="snippetSchema.json.default">
        <source xml:lang="en">Empty snippet</source>
        <target state="translated">Пустой фрагмент</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json">
        <source xml:lang="en">User snippet configuration</source>
        <target state="translated">Настройка фрагмента пользователя</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.prefix">
        <source xml:lang="en">The prefix to used when selecting the snippet in intellisense</source>
        <target state="translated">Префикс, используемый при выборе фрагмента в Intellisense.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.body">
        <source xml:lang="en">The snippet content. Use '$1', '${1:defaultText}' to define cursor positions, use '$0' for the final cursor position. Insert variable values with '${varName}' and '${varName:defaultText}', e.g. 'This is file: $TM_FILENAME'.</source>
        <target state="translated">Содержимое фрагмента. Используйте "$1", "${1:defaultText}", чтобы определить позиции курсора, и "$0" для конечной позиции курсора. Вставьте значения переменных с помощью "${varName}" и "${varName:defaultText}", например 'This is file: $TM_FILENAME'.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">Описание фрагмента.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="translated">Список имен языков, к которым относится этот фрагмент, например, "typescript,javascript".</target>
      </trans-unit>
    </body>
  </file>
