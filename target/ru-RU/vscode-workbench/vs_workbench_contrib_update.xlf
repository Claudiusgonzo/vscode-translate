  <file original="src/vs/workbench/contrib/update/browser/update" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotes">
        <source xml:lang="en">Release Notes</source>
        <target state="translated">Заметки о выпуске</target>
      </trans-unit>
      <trans-unit id="showReleaseNotes">
        <source xml:lang="en">Show Release Notes</source>
        <target state="translated">Показать заметки о выпуске</target>
      </trans-unit>
      <trans-unit id="read the release notes">
        <source xml:lang="en">Welcome to {0} v{1}! Would you like to read the Release Notes?</source>
        <target state="translated">Вас приветствует {0} v{1}! Вы хотите прочитать заметки о выпуске?</target>
      </trans-unit>
      <trans-unit id="licenseChanged">
        <source xml:lang="en">Our license terms have changed, please click [here]({0}) to go through them.</source>
        <target state="translated">Наши условия лицензии изменились. Чтобы ознакомиться с ними, щелкните [здесь]({0}).</target>
      </trans-unit>
      <trans-unit id="updateIsReady">
        <source xml:lang="en">New {0} update available.</source>
        <target state="translated">Доступно новое обновление {0}.</target>
      </trans-unit>
      <trans-unit id="checkingForUpdates">
        <source xml:lang="en">Checking for Updates...</source>
        <target state="translated">Идет проверка наличия обновлений...</target>
      </trans-unit>
      <trans-unit id="update service">
        <source xml:lang="en">Update Service</source>
        <target state="translated">Обновить службу</target>
      </trans-unit>
      <trans-unit id="noUpdatesAvailable">
        <source xml:lang="en">There are currently no updates available.</source>
        <target state="translated">Доступные обновления отсутствуют.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">ОК</target>
      </trans-unit>
      <trans-unit id="thereIsUpdateAvailable">
        <source xml:lang="en">There is an available update.</source>
        <target state="translated">Доступно обновление.</target>
      </trans-unit>
      <trans-unit id="download update">
        <source xml:lang="en">Download Update</source>
        <target state="translated">Скачать обновление</target>
      </trans-unit>
      <trans-unit id="later">
        <source xml:lang="en">Later</source>
        <target state="translated">Позже</target>
      </trans-unit>
      <trans-unit id="updateAvailable">
        <source xml:lang="en">There's an update available: {0} {1}</source>
        <target state="translated">Доступно обновление: {0} {1}</target>
      </trans-unit>
      <trans-unit id="installUpdate">
        <source xml:lang="en">Install Update</source>
        <target state="translated">Установить обновление</target>
      </trans-unit>
      <trans-unit id="updateInstalling">
        <source xml:lang="en">{0} {1} is being installed in the background; we'll let you know when it's done.</source>
        <target state="translated">{0} {1} устанавливается в фоновом режиме, мы сообщим вам о завершении.</target>
      </trans-unit>
      <trans-unit id="updateNow">
        <source xml:lang="en">Update Now</source>
        <target state="translated">Обновить сейчас</target>
      </trans-unit>
      <trans-unit id="updateAvailableAfterRestart">
        <source xml:lang="en">Restart {0} to apply the latest update.</source>
        <target state="translated">Перезапустите {0}, чтобы применить последнее обновление.</target>
      </trans-unit>
      <trans-unit id="checkForUpdates">
        <source xml:lang="en">Check for Updates...</source>
        <target state="translated">Проверить наличие обновлений...</target>
      </trans-unit>
      <trans-unit id="DownloadingUpdate">
        <source xml:lang="en">Downloading Update...</source>
        <target state="translated">Скачивается обновление...</target>
      </trans-unit>
      <trans-unit id="installUpdate...">
        <source xml:lang="en">Install Update...</source>
        <target state="translated">Установить обновление...</target>
      </trans-unit>
      <trans-unit id="installingUpdate">
        <source xml:lang="en">Installing Update...</source>
        <target state="translated">Идет установка обновления...</target>
      </trans-unit>
      <trans-unit id="restartToUpdate">
        <source xml:lang="en">Restart to Update (1)</source>
        <target state="translated">Перезапустить для обновления (1)</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/update/browser/releaseNotesEditor" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotesInputName">
        <source xml:lang="en">Release Notes: {0}</source>
        <target state="translated">Заметки о выпуске: {0}</target>
      </trans-unit>
      <trans-unit id="unassigned">
        <source xml:lang="en">unassigned</source>
        <target state="translated">не присвоено</target>
      </trans-unit>
    </body>
  </file>
