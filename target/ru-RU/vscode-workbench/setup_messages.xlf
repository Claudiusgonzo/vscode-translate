<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" target-language="ru" datatype="plaintext">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target state="translated">Добавить действие "Открыть с помощью %1" в контекстное меню файла проводника Windows</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target state="translated">Добавить действие "Открыть с помощью %1" в контекстное меню каталога проводника</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target state="translated">Зарегистрировать %1 в качестве редактора для поддерживаемых типов файлов</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target state="translated">Добавить в PATH (требуется перезагрузка оболочки)</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target state="translated">Запустить %1 после установки</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target state="translated">Другое:</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target state="translated">Исходный файл %1</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target state="translated">Открыть с п&amp;омощью %1</target>
      </trans-unit>
    </body>
  </file>
</xliff>