<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/merge-conflict/package" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Merge-Konflikt</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Highlighting and commands for inline merge conflicts.</source>
        <target state="translated">Hervorhebung und Befehle für Inline-Mergingkonflikte.</target>
      </trans-unit>
      <trans-unit id="command.category">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Merge-Konflikt</target>
      </trans-unit>
      <trans-unit id="command.accept.all-current">
        <source xml:lang="en">Accept All Current</source>
        <target state="translated">Alle aktuellen akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.all-incoming">
        <source xml:lang="en">Accept All Incoming</source>
        <target state="translated">Alle eingehenden akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.all-both">
        <source xml:lang="en">Accept All Both</source>
        <target state="translated">Alle beide akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.current">
        <source xml:lang="en">Accept Current</source>
        <target state="translated">Aktuelles akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.incoming">
        <source xml:lang="en">Accept Incoming</source>
        <target state="translated">Eingehendes akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.selection">
        <source xml:lang="en">Accept Selection</source>
        <target state="translated">Auswahl akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.accept.both">
        <source xml:lang="en">Accept Both</source>
        <target state="translated">Beides akzeptieren</target>
      </trans-unit>
      <trans-unit id="command.next">
        <source xml:lang="en">Next Conflict</source>
        <target state="translated">Nächster Konflikt</target>
      </trans-unit>
      <trans-unit id="command.previous">
        <source xml:lang="en">Previous Conflict</source>
        <target state="translated">Vorheriger Konflikt</target>
      </trans-unit>
      <trans-unit id="command.compare">
        <source xml:lang="en">Compare Current Conflict</source>
        <target state="translated">Aktuellen Konflikt vergleichen</target>
      </trans-unit>
      <trans-unit id="config.title">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Merge-Konflikt</target>
      </trans-unit>
      <trans-unit id="config.autoNavigateNextConflictEnabled">
        <source xml:lang="en">Whether to automatically navigate to the next merge conflict after resolving a merge conflict.</source>
        <target state="translated">Gibt an, ob automatisch zum nächsten Mergekonflikt navigiert werden soll, nachdem ein Mergekonflikt behoben wurde.</target>
      </trans-unit>
      <trans-unit id="config.codeLensEnabled">
        <source xml:lang="en">Create a Code Lens for merge conflict blocks within editor.</source>
        <target state="translated">CodeLens für Mergingkonfliktblöcke im Editor erstellen.</target>
      </trans-unit>
      <trans-unit id="config.decoratorsEnabled">
        <source xml:lang="en">Create decorators for merge conflict blocks within editor.</source>
        <target state="translated">Decorator-Elemente für Mergingkonfliktblöcke im Editor erstellen.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition">
        <source xml:lang="en">Controls where the diff view should be opened when comparing changes in merge conflicts.</source>
        <target state="translated">Steuert, wo die Vergleichsansicht beim Vergleich von Änderungen in Zusammenführungskonflikten geöffnet wird.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.current">
        <source xml:lang="en">Open the diff view in the current editor group.</source>
        <target state="translated">Hiermit wird die Vergleichsansicht in der aktuellen Editorgruppe geöffnet.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.beside">
        <source xml:lang="en">Open the diff view next to the current editor group.</source>
        <target state="translated">Hiermit öffnen Sie die Vergleichsansicht neben der aktuellen Editorgruppe.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.below">
        <source xml:lang="en">Open the diff view below the current editor group.</source>
        <target state="translated">Hiermit wird die Vergleichsansicht unterhalb der aktuellen Editorgruppe geöffnet.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/codelensProvider" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="acceptCurrentChange">
        <source xml:lang="en">Accept Current Change</source>
        <target state="translated">Aktuelle Änderung akzeptieren</target>
      </trans-unit>
      <trans-unit id="acceptIncomingChange">
        <source xml:lang="en">Accept Incoming Change</source>
        <target state="translated">Eingehende Änderung akzeptieren</target>
      </trans-unit>
      <trans-unit id="acceptBothChanges">
        <source xml:lang="en">Accept Both Changes</source>
        <target state="translated">Beide Änderungen akzeptieren</target>
      </trans-unit>
      <trans-unit id="compareChanges">
        <source xml:lang="en">Compare Changes</source>
        <target state="translated">Änderungen vergleichen</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/commandHandler" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="cursorNotInConflict">
        <source xml:lang="en">Editor cursor is not within a merge conflict</source>
        <target state="translated">Der Editor-Cursor ist nicht innerhalb eines Mergingkonflikts</target>
      </trans-unit>
      <trans-unit id="compareChangesTitle">
        <source xml:lang="en">{0}: Current Changes ⟷ Incoming Changes</source>
        <target state="translated">{0}: Aktuelle Änderungen ⟷ Eingehende Änderungen</target>
      </trans-unit>
      <trans-unit id="cursorOnCommonAncestorsRange">
        <source xml:lang="en">Editor cursor is within the common ancestors block, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Der Editor-Cursor ist innerhalb des Blocks gemeinsamer Vorgänger, verschieben Sie ihn entweder in den Block "aktuell" oder "eingehend". </target>
      </trans-unit>
      <trans-unit id="cursorOnSplitterRange">
        <source xml:lang="en">Editor cursor is within the merge conflict splitter, please move it to either the "current" or "incoming" block</source>
        <target state="translated">Der Editor-Cursor ist innerhalb der Mergingkonfliktaufteilung, verschieben Sie ihn entweder in den Block "aktuell" oder "eingehend".</target>
      </trans-unit>
      <trans-unit id="noConflicts">
        <source xml:lang="en">No merge conflicts found in this file</source>
        <target state="translated">Keine Merge-Konflikte in dieser Datei gefunden</target>
      </trans-unit>
      <trans-unit id="noOtherConflictsInThisFile">
        <source xml:lang="en">No other merge conflicts within this file</source>
        <target state="translated">Keine weiteren Merge-Konflikte in dieser Datei</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/mergeDecorator" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="currentChange">
        <source xml:lang="en">(Current Change)</source>
        <target state="translated">(Aktuelle Änderung)</target>
      </trans-unit>
      <trans-unit id="incomingChange">
        <source xml:lang="en">(Incoming Change)</source>
        <target state="translated">(Eingehende Änderung)</target>
      </trans-unit>
    </body>
  </file>
