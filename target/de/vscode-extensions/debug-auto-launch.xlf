<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/debug-auto-launch/package" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Node Debug Auto-attach</source>
        <target state="translated">Node-Debugging – Automatisches Anfügen</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Helper for auto-attach feature when node-debug extensions are not active.</source>
        <target state="translated">Hilfsprogramm für Feature "Automatisches Anfügen", wenn Node-Debugging-Erweiterungen nicht aktiv sind.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.description">
        <source xml:lang="en">Automatically attach node debugger when node.js was launched in debug mode from integrated terminal.</source>
        <target state="translated">Node-Debugger automatisch anfügen, wenn Node.js über das integrierte Terminal im Debugmodus gestartet wurde.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.disabled.description">
        <source xml:lang="en">Auto attach is disabled and not shown in status bar.</source>
        <target state="translated">Automatisches Anfügen ist deaktiviert und wird nicht in der Statusleiste angezeigt.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.on.description">
        <source xml:lang="en">Auto attach is active.</source>
        <target state="translated">Automatisches Anfügen ist aktiv.</target>
      </trans-unit>
      <trans-unit id="debug.node.autoAttach.off.description">
        <source xml:lang="en">Auto attach is inactive.</source>
        <target state="translated">Automatisches Anfügen ist nicht aktiv.</target>
      </trans-unit>
      <trans-unit id="toggle.auto.attach">
        <source xml:lang="en">Toggle Auto Attach</source>
        <target state="translated">Automatisches Anfügen umschalten</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/debug-auto-launch/dist/extension" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="status.text.auto.attach.on">
        <source xml:lang="en">Auto Attach: On</source>
        <target state="translated">Automatisches Anfügen: Eingeschaltet</target>
      </trans-unit>
      <trans-unit id="status.text.auto.attach.off">
        <source xml:lang="en">Auto Attach: Off</source>
        <target state="translated">Automatisches Anfügen: Ausgeschaltet</target>
      </trans-unit>
      <trans-unit id="status.tooltip.auto.attach">
        <source xml:lang="en">Automatically attach to node.js processes in debug mode</source>
        <target state="translated">Im Debugmodus  automatisch an Node.js-Prozesse anfügen</target>
      </trans-unit>
    </body>
  </file>
</xliff>