  <file original="src/vs/workbench/contrib/format/browser/formatActionsNone" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document</source>
        <target state="translated">Dokument formatieren</target>
      </trans-unit>
      <trans-unit id="no.provider">
        <source xml:lang="en">There is no formatter for '{0}' files installed.</source>
        <target state="translated">Es ist kein Formatierer für {0}-Dateien installiert.</target>
      </trans-unit>
      <trans-unit id="install.formatter">
        <source xml:lang="en">Install Formatter...</source>
        <target state="translated">Formatierer installieren...</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/format/browser/formatActionsMultiple" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="nullFormatterDescription">
        <source xml:lang="en">None</source>
        <target state="translated">NONE</target>
      </trans-unit>
      <trans-unit id="miss">
        <source xml:lang="en">Extension '{0}' cannot format '{1}'</source>
        <target state="translated">Erweiterung '{0}' kann '{1}' nicht formatieren</target>
      </trans-unit>
      <trans-unit id="config.needed">
        <source xml:lang="en">There are multiple formatters for '{0}' files. Select a default formatter to continue.</source>
        <target state="translated">Es gibt mehrere Formatierer für {0}-Dateien. Wählen Sie einen Standardformatierer aus, um fortzufahren.</target>
      </trans-unit>
      <trans-unit id="config.bad">
        <source xml:lang="en">Extension '{0}' is configured as formatter but not available. Select a different default formatter to continue.</source>
        <target state="translated">Die Erweiterung "{0}" ist als Formatierer konfiguriert, aber nicht verfügbar. Wählen Sie einen anderen Standardformatierer aus.</target>
      </trans-unit>
      <trans-unit id="do.config">
        <source xml:lang="en">Configure...</source>
        <target state="translated">Konfigurieren …</target>
      </trans-unit>
      <trans-unit id="select">
        <source xml:lang="en">Select a default formatter for '{0}' files</source>
        <target state="translated">Standardformatierer für {0}-Dateien auswählen</target>
      </trans-unit>
      <trans-unit id="formatter.default">
        <source xml:lang="en">Defines a default formatter which takes precedence over all other formatter settings. Must be the identifier of an extension contributing a formatter.</source>
        <target state="translated">Definiert einen Standardformatierer, der Vorrang gegenüber allen anderen Formatierereinstellungen hat. Muss der Bezeichner einer Erweiterung sein, die zu einem Formatierer gehört.</target>
      </trans-unit>
      <trans-unit id="def">
        <source xml:lang="en">(default)</source>
        <target state="translated">(Standard)</target>
      </trans-unit>
      <trans-unit id="config">
        <source xml:lang="en">Configure Default Formatter...</source>
        <target state="translated">Standardformatierer konfigurieren …</target>
      </trans-unit>
      <trans-unit id="format.placeHolder">
        <source xml:lang="en">Select a formatter</source>
        <target state="translated">Formatierer auswählen</target>
      </trans-unit>
      <trans-unit id="formatDocument.label.multiple">
        <source xml:lang="en">Format Document With...</source>
        <target state="translated">Dokument formatieren mit ...</target>
      </trans-unit>
      <trans-unit id="formatSelection.label.multiple">
        <source xml:lang="en">Format Selection With...</source>
        <target state="translated">Auswahl formatieren mit ...</target>
      </trans-unit>
    </body>
  </file>
