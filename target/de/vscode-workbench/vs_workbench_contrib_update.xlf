  <file original="src/vs/workbench/contrib/update/browser/update" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotes">
        <source xml:lang="en">Release Notes</source>
        <target state="translated">Anmerkungen zu dieser Version</target>
      </trans-unit>
      <trans-unit id="showReleaseNotes">
        <source xml:lang="en">Show Release Notes</source>
        <target state="translated">Anmerkungen zu dieser Version anzeigen</target>
      </trans-unit>
      <trans-unit id="read the release notes">
        <source xml:lang="en">Welcome to {0} v{1}! Would you like to read the Release Notes?</source>
        <target state="translated">Willkommen bei {0} v{1}! Möchten Sie die Hinweise zu dieser Version lesen?</target>
      </trans-unit>
      <trans-unit id="licenseChanged">
        <source xml:lang="en">Our license terms have changed, please click [here]({0}) to go through them.</source>
        <target state="translated">Unsere Lizenzbedingungen haben sich geändert. Bitte klicken Sie [hier]({0}), um die neuen Bedingungen zu lesen.</target>
      </trans-unit>
      <trans-unit id="updateIsReady">
        <source xml:lang="en">New {0} update available.</source>
        <target state="translated">Neues {0}-Update verfügbar.</target>
      </trans-unit>
      <trans-unit id="checkingForUpdates">
        <source xml:lang="en">Checking for Updates...</source>
        <target state="translated">Es wird nach Updates gesucht...</target>
      </trans-unit>
      <trans-unit id="update service">
        <source xml:lang="en">Update Service</source>
        <target state="translated">Dienst aktualisieren</target>
      </trans-unit>
      <trans-unit id="noUpdatesAvailable">
        <source xml:lang="en">There are currently no updates available.</source>
        <target state="translated">Zurzeit sind keine Updates verfügbar.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">OK</target>
      </trans-unit>
      <trans-unit id="thereIsUpdateAvailable">
        <source xml:lang="en">There is an available update.</source>
        <target state="translated">Ein Update ist verfügbar.</target>
      </trans-unit>
      <trans-unit id="download update">
        <source xml:lang="en">Download Update</source>
        <target state="translated">Update herunterladen</target>
      </trans-unit>
      <trans-unit id="later">
        <source xml:lang="en">Later</source>
        <target state="translated">Später</target>
      </trans-unit>
      <trans-unit id="updateAvailable">
        <source xml:lang="en">There's an update available: {0} {1}</source>
        <target state="translated">Ein Update ist verfügbar: {0} {1}</target>
      </trans-unit>
      <trans-unit id="installUpdate">
        <source xml:lang="en">Install Update</source>
        <target state="translated">Update installieren</target>
      </trans-unit>
      <trans-unit id="updateInstalling">
        <source xml:lang="en">{0} {1} is being installed in the background; we'll let you know when it's done.</source>
        <target state="translated">{0} {1} wird im Hintergrund installiert. Wir informieren Sie, wenn der Vorgang abgeschlossen ist.</target>
      </trans-unit>
      <trans-unit id="updateNow">
        <source xml:lang="en">Update Now</source>
        <target state="translated">Jetzt aktualisieren</target>
      </trans-unit>
      <trans-unit id="updateAvailableAfterRestart">
        <source xml:lang="en">Restart {0} to apply the latest update.</source>
        <target state="translated">Starten Sie {0} neu, um das neueste Update zu installieren.</target>
      </trans-unit>
      <trans-unit id="checkForUpdates">
        <source xml:lang="en">Check for Updates...</source>
        <target state="translated">Nach Aktualisierungen suchen...</target>
      </trans-unit>
      <trans-unit id="DownloadingUpdate">
        <source xml:lang="en">Downloading Update...</source>
        <target state="translated">Das Update wird heruntergeladen...</target>
      </trans-unit>
      <trans-unit id="installUpdate...">
        <source xml:lang="en">Install Update...</source>
        <target state="translated">Update installieren …</target>
      </trans-unit>
      <trans-unit id="installingUpdate">
        <source xml:lang="en">Installing Update...</source>
        <target state="translated">Update wird installiert...</target>
      </trans-unit>
      <trans-unit id="restartToUpdate">
        <source xml:lang="en">Restart to Update (1)</source>
        <target state="translated">Neustart zum Updaten (1)</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/update/browser/releaseNotesEditor" source-language="en" target-language="de" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotesInputName">
        <source xml:lang="en">Release Notes: {0}</source>
        <target state="translated">Anmerkungen zu dieser Version: {0}</target>
      </trans-unit>
      <trans-unit id="unassigned">
        <source xml:lang="en">unassigned</source>
        <target state="translated">Nicht zugewiesen</target>
      </trans-unit>
    </body>
  </file>
