<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" target-language="bg" datatype="plaintext">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target state="translated">Добавяне на действие „Отваряне с %1“ в контекстното меню за файлове</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target state="translated">Добавяне на действие „Отваряне с %1“ в контекстното меню за папки</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target state="translated">Регистриране на „%1“ като редактор за поддържаните файлови типове</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target state="new">Add to PATH (requires shell restart)</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target state="translated">Пускане на „%1“ след инсталацията</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target state="translated">Други:</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target state="translated">Файл с изходен код – %1</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target state="new">Open w&amp;ith %1</target>
      </trans-unit>
    </body>
  </file>
</xliff>