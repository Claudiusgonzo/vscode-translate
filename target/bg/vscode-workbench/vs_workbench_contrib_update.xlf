  <file original="src/vs/workbench/contrib/update/browser/update" source-language="en" target-language="bg" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotes">
        <source xml:lang="en">Release Notes</source>
        <target state="translated">Бележки за изданието</target>
      </trans-unit>
      <trans-unit id="showReleaseNotes">
        <source xml:lang="en">Show Release Notes</source>
        <target state="translated">Показване на бележките за изданието</target>
      </trans-unit>
      <trans-unit id="read the release notes">
        <source xml:lang="en">Welcome to {0} v{1}! Would you like to read the Release Notes?</source>
        <target state="translated">Добре дошли в {0} версия {1}! Искате ли да прочетете бележките за това издание?</target>
      </trans-unit>
      <trans-unit id="licenseChanged">
        <source xml:lang="en">Our license terms have changed, please click [here]({0}) to go through them.</source>
        <target state="translated">Условията на лиценза ни са променени. Моля, натиснете [тук]({0}), за да ги прегледате.</target>
      </trans-unit>
      <trans-unit id="updateIsReady">
        <source xml:lang="en">New {0} update available.</source>
        <target state="translated">Има налично обновление на {0}.</target>
      </trans-unit>
      <trans-unit id="checkingForUpdates">
        <source xml:lang="en">Checking for Updates...</source>
        <target state="new">Checking for Updates...</target>
      </trans-unit>
      <trans-unit id="update service">
        <source xml:lang="en">Update Service</source>
        <target state="translated">Услуга за обновяване</target>
      </trans-unit>
      <trans-unit id="noUpdatesAvailable">
        <source xml:lang="en">There are currently no updates available.</source>
        <target state="translated">В момента няма налични обновления.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">Добре</target>
      </trans-unit>
      <trans-unit id="thereIsUpdateAvailable">
        <source xml:lang="en">There is an available update.</source>
        <target state="translated">Има налично обновление.</target>
      </trans-unit>
      <trans-unit id="download update">
        <source xml:lang="en">Download Update</source>
        <target state="new">Download Update</target>
      </trans-unit>
      <trans-unit id="later">
        <source xml:lang="en">Later</source>
        <target state="translated">По-късно</target>
      </trans-unit>
      <trans-unit id="updateAvailable">
        <source xml:lang="en">There's an update available: {0} {1}</source>
        <target state="translated">Има налично обновление: {0} {1}</target>
      </trans-unit>
      <trans-unit id="installUpdate">
        <source xml:lang="en">Install Update</source>
        <target state="translated">Инсталиране на обновлението</target>
      </trans-unit>
      <trans-unit id="updateInstalling">
        <source xml:lang="en">{0} {1} is being installed in the background; we'll let you know when it's done.</source>
        <target state="translated">{0} {1} се инсталира на заден фон. Ще Ви уведомим, когато обновяването завърши.</target>
      </trans-unit>
      <trans-unit id="updateNow">
        <source xml:lang="en">Update Now</source>
        <target state="translated">Обновяване сега</target>
      </trans-unit>
      <trans-unit id="updateAvailableAfterRestart">
        <source xml:lang="en">Restart {0} to apply the latest update.</source>
        <target state="translated">Рестартирайте {0}, за да се приложи новото обновление.</target>
      </trans-unit>
      <trans-unit id="checkForUpdates">
        <source xml:lang="en">Check for Updates...</source>
        <target state="translated">Проверка за обновления…</target>
      </trans-unit>
      <trans-unit id="DownloadingUpdate">
        <source xml:lang="en">Downloading Update...</source>
        <target state="translated">Сваляне на обновлението…</target>
      </trans-unit>
      <trans-unit id="installUpdate...">
        <source xml:lang="en">Install Update...</source>
        <target state="translated">Инсталиране на обновлението…</target>
      </trans-unit>
      <trans-unit id="installingUpdate">
        <source xml:lang="en">Installing Update...</source>
        <target state="translated">Инсталиране на обновлението…</target>
      </trans-unit>
      <trans-unit id="restartToUpdate">
        <source xml:lang="en">Restart to Update (1)</source>
        <target state="new">Restart to Update (1)</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/update/browser/releaseNotesEditor" source-language="en" target-language="bg" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotesInputName">
        <source xml:lang="en">Release Notes: {0}</source>
        <target state="translated">Бележки за изданието: {0}</target>
      </trans-unit>
      <trans-unit id="unassigned">
        <source xml:lang="en">unassigned</source>
        <target state="translated">не е зададено</target>
      </trans-unit>
    </body>
  </file>
