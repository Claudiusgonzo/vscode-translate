<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="extensions/merge-conflict/package" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="displayName">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Fusionar conflicto</target>
      </trans-unit>
      <trans-unit id="description">
        <source xml:lang="en">Highlighting and commands for inline merge conflicts.</source>
        <target state="translated">Resaltado y comandos para conflictos de fusión insertada.</target>
      </trans-unit>
      <trans-unit id="command.category">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Fusionar conflicto</target>
      </trans-unit>
      <trans-unit id="command.accept.all-current">
        <source xml:lang="en">Accept All Current</source>
        <target state="translated">Aceptar todo actual</target>
      </trans-unit>
      <trans-unit id="command.accept.all-incoming">
        <source xml:lang="en">Accept All Incoming</source>
        <target state="translated">Aceptar todos los entrantes</target>
      </trans-unit>
      <trans-unit id="command.accept.all-both">
        <source xml:lang="en">Accept All Both</source>
        <target state="translated">Aceptar ambos</target>
      </trans-unit>
      <trans-unit id="command.accept.current">
        <source xml:lang="en">Accept Current</source>
        <target state="translated">Aceptar actuales</target>
      </trans-unit>
      <trans-unit id="command.accept.incoming">
        <source xml:lang="en">Accept Incoming</source>
        <target state="translated">Aceptar entrantes</target>
      </trans-unit>
      <trans-unit id="command.accept.selection">
        <source xml:lang="en">Accept Selection</source>
        <target state="translated">Aceptar selección</target>
      </trans-unit>
      <trans-unit id="command.accept.both">
        <source xml:lang="en">Accept Both</source>
        <target state="translated">Aceptar ambos</target>
      </trans-unit>
      <trans-unit id="command.next">
        <source xml:lang="en">Next Conflict</source>
        <target state="translated">Siguiente conflicto</target>
      </trans-unit>
      <trans-unit id="command.previous">
        <source xml:lang="en">Previous Conflict</source>
        <target state="translated">Conflicto anterior</target>
      </trans-unit>
      <trans-unit id="command.compare">
        <source xml:lang="en">Compare Current Conflict</source>
        <target state="translated">Comparar conflicto actual</target>
      </trans-unit>
      <trans-unit id="config.title">
        <source xml:lang="en">Merge Conflict</source>
        <target state="translated">Fusionar conflicto</target>
      </trans-unit>
      <trans-unit id="config.autoNavigateNextConflictEnabled">
        <source xml:lang="en">Whether to automatically navigate to the next merge conflict after resolving a merge conflict.</source>
        <target state="translated">Indica si, después de resolver un conflicto de fusión mediante combinación, se va automáticamente al siguiente conflicto de este tipo.</target>
      </trans-unit>
      <trans-unit id="config.codeLensEnabled">
        <source xml:lang="en">Create a Code Lens for merge conflict blocks within editor.</source>
        <target state="translated">Cree CodeLens para los bloques de conflictos de fusión mediante combinación en el editor.</target>
      </trans-unit>
      <trans-unit id="config.decoratorsEnabled">
        <source xml:lang="en">Create decorators for merge conflict blocks within editor.</source>
        <target state="translated">Cree elementos Decorator para los bloques de conflictos de fusión mediante combinación en el editor.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition">
        <source xml:lang="en">Controls where the diff view should be opened when comparing changes in merge conflicts.</source>
        <target state="translated">Controla dónde se debe abrir la vista de diferencias al comparar los cambios en los conflictos de combinación.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.current">
        <source xml:lang="en">Open the diff view in the current editor group.</source>
        <target state="translated">Abra la vista de diferencias en el grupo de editor actual.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.beside">
        <source xml:lang="en">Open the diff view next to the current editor group.</source>
        <target state="translated">Abra la vista de diferencias junto al grupo de editor actual.</target>
      </trans-unit>
      <trans-unit id="config.diffViewPosition.below">
        <source xml:lang="en">Open the diff view below the current editor group.</source>
        <target state="translated">Abra la vista de diferencias debajo del grupo de editor actual.</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/codelensProvider" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="acceptCurrentChange">
        <source xml:lang="en">Accept Current Change</source>
        <target state="translated">Aceptar cambio actual</target>
      </trans-unit>
      <trans-unit id="acceptIncomingChange">
        <source xml:lang="en">Accept Incoming Change</source>
        <target state="translated">Aceptar cambio entrante</target>
      </trans-unit>
      <trans-unit id="acceptBothChanges">
        <source xml:lang="en">Accept Both Changes</source>
        <target state="translated">Aceptar ambos cambios</target>
      </trans-unit>
      <trans-unit id="compareChanges">
        <source xml:lang="en">Compare Changes</source>
        <target state="translated">Comparar cambios</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/commandHandler" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="cursorNotInConflict">
        <source xml:lang="en">Editor cursor is not within a merge conflict</source>
        <target state="translated">El cursor de edición no se encuentra en un conflicto de fusión</target>
      </trans-unit>
      <trans-unit id="compareChangesTitle">
        <source xml:lang="en">{0}: Current Changes ⟷ Incoming Changes</source>
        <target state="translated">{0}: Cambios actuales ⟷ Cambios entrantes</target>
      </trans-unit>
      <trans-unit id="cursorOnCommonAncestorsRange">
        <source xml:lang="en">Editor cursor is within the common ancestors block, please move it to either the "current" or "incoming" block</source>
        <target state="translated">El cursor del editor está dentro del bloque de ancestros comunes, por favor muévalo al bloque "actual" o al "entrante"</target>
      </trans-unit>
      <trans-unit id="cursorOnSplitterRange">
        <source xml:lang="en">Editor cursor is within the merge conflict splitter, please move it to either the "current" or "incoming" block</source>
        <target state="translated">El cursor del editor está dentro del separador de conflictos de fusión, muévalo al bloque "actual" o al "entrante" </target>
      </trans-unit>
      <trans-unit id="noConflicts">
        <source xml:lang="en">No merge conflicts found in this file</source>
        <target state="translated">No se encontraron conflictos en este archivo</target>
      </trans-unit>
      <trans-unit id="noOtherConflictsInThisFile">
        <source xml:lang="en">No other merge conflicts within this file</source>
        <target state="translated">No hay más conflictos en este archivo</target>
      </trans-unit>
    </body>
  </file>
  <file original="extensions/merge-conflict/dist/mergeDecorator" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="currentChange">
        <source xml:lang="en">(Current Change)</source>
        <target state="translated">(Cambio actual)</target>
      </trans-unit>
      <trans-unit id="incomingChange">
        <source xml:lang="en">(Incoming Change)</source>
        <target state="translated">(Cambio entrante)</target>
      </trans-unit>
    </body>
  </file>
</xliff>