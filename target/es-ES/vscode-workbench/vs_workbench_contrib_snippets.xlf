  <file original="src/vs/workbench/contrib/snippets/browser/configureSnippets" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="global.scope">
        <source xml:lang="en">(global)</source>
        <target state="translated">(global)</target>
      </trans-unit>
      <trans-unit id="global.1">
        <source xml:lang="en">({0})</source>
        <target state="translated">({0})</target>
      </trans-unit>
      <trans-unit id="name">
        <source xml:lang="en">Type snippet file name</source>
        <target state="translated">Escriba el nombre del archivo de fragmento de código</target>
      </trans-unit>
      <trans-unit id="bad_name1">
        <source xml:lang="en">Invalid file name</source>
        <target state="translated">Nombre de archivo no válido</target>
      </trans-unit>
      <trans-unit id="bad_name2">
        <source xml:lang="en">'{0}' is not a valid file name</source>
        <target state="translated">"{0}" no es un nombre de archivo válido</target>
      </trans-unit>
      <trans-unit id="bad_name3">
        <source xml:lang="en">'{0}' already exists</source>
        <target state="translated">"{0}" ya existe</target>
      </trans-unit>
      <trans-unit id="new.global_scope">
        <source xml:lang="en">global</source>
        <target state="translated">GLOBAL</target>
      </trans-unit>
      <trans-unit id="new.global">
        <source xml:lang="en">New Global Snippets file...</source>
        <target state="translated">Nuevo archivo de fragmentos globales...</target>
      </trans-unit>
      <trans-unit id="new.workspace_scope">
        <source xml:lang="en">{0} workspace</source>
        <target state="translated">área de trabajo de {0}</target>
      </trans-unit>
      <trans-unit id="new.folder">
        <source xml:lang="en">New Snippets file for '{0}'...</source>
        <target state="translated">Nuevo archivo de fragmentos para "{0}"...</target>
      </trans-unit>
      <trans-unit id="group.global">
        <source xml:lang="en">Existing Snippets</source>
        <target state="translated">Fragmentos existentes</target>
      </trans-unit>
      <trans-unit id="new.global.sep">
        <source xml:lang="en">New Snippets</source>
        <target state="translated">Nuevos fragmentos de código</target>
      </trans-unit>
      <trans-unit id="openSnippet.pickLanguage">
        <source xml:lang="en">Select Snippets File or Create Snippets</source>
        <target state="translated">Seleccione Archivo de fragmentos o Crear fragmentos de código</target>
      </trans-unit>
      <trans-unit id="openSnippet.label">
        <source xml:lang="en">Configure User Snippets</source>
        <target state="translated">Configurar fragmentos de usuario </target>
      </trans-unit>
      <trans-unit id="preferences">
        <source xml:lang="en">Preferences</source>
        <target state="translated">Preferencias</target>
      </trans-unit>
      <trans-unit id="miOpenSnippets">
        <source xml:lang="en">User &amp;&amp;Snippets</source>
        <target state="translated">&amp;&amp;Fragmentos de código del usuario</target>
        <note>&amp;&amp; denotes a mnemonic</note>
      </trans-unit>
      <trans-unit id="userSnippets">
        <source xml:lang="en">User Snippets</source>
        <target state="translated">Fragmentos de código de usuario</target>
      </trans-unit>
    </body>
  </file>
  <file original="src/vs/workbench/contrib/snippets/browser/snippetsService" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="invalid.path.0">
        <source xml:lang="en">Expected string in `contributes.{0}.path`. Provided value: {1}</source>
        <target state="translated">Se esperaba una cadena en 'contributes.{0}.path'. Valor proporcionado: {1}</target>
      </trans-unit>
      <trans-unit id="invalid.language.0">
        <source xml:lang="en">When omitting the language, the value of `contributes.{0}.path` must be a `.code-snippets`-file. Provided value: {1}</source>
        <target state="translated">Al omitir el lenguaje, el valor de 'contributes. {0}. Path' debe ser un archivo '. Code-snippets'. Valor proporcionado: {1}</target>
      </trans-unit>
      <trans-unit id="invalid.language">
        <source xml:lang="en">Unknown language in `contributes.{0}.language`. Provided value: {1}</source>
        <target state="translated">Lenguaje desconocido en "contributes.{0}.language". Valor proporcionado: {1}</target>
      </trans-unit>
      <trans-unit id="invalid.path.1">
        <source xml:lang="en">Expected `contributes.{0}.path` ({1}) to be included inside extension's folder ({2}). This might make the extension non-portable.</source>
        <target state="translated">Se esperaba que "contributes.{0}.path" ({1}) se incluyera en la carpeta de la extensión ({2}). Esto puede hacer que la extensión no sea portátil.</target>
      </trans-unit>
      <trans-unit id="vscode.extension.contributes.snippets">
        <source xml:lang="en">Contributes snippets.</source>
        <target state="translated">Aporta fragmentos de código.</target>
      </trans-unit>
      <trans-unit id="vscode.extension.contributes.snippets-language">
        <source xml:lang="en">Language identifier for which this snippet is contributed to.</source>
        <target state="translated">Identificador del lenguaje al que se aporta este fragmento de código.</target>
      </trans-unit>
      <trans-unit id="vscode.extension.contributes.snippets-path">
        <source xml:lang="en">Path of the snippets file. The path is relative to the extension folder and typically starts with './snippets/'.</source>
        <target state="translated">Ruta de acceso del archivo de fragmentos de código. La ruta es relativa a la carpeta de extensión y normalmente empieza por "./snippets/".</target>
      </trans-unit>
      <trans-unit id="badVariableUse">
        <source xml:lang="en">One or more snippets from the extension '{0}' very likely confuse snippet-variables and snippet-placeholders (see https://code.visualstudio.com/docs/editor/userdefinedsnippets#_snippet-syntax for more details)</source>
        <target state="translated">Es muy probable que uno o más fragmentos de la extensión "{0}" confundan las variables de fragmento de código y los marcadores de posición de fragmento de código (consulte https://code.visualstudio.com/docs/editor/userdefinedsnippets#_snippet-syntax para obtener más detalles)</target>
      </trans-unit>
      <trans-unit id="badFile">
        <source xml:lang="en">The snippet file "{0}" could not be read.</source>
        <target state="translated">No se pudo leer el archivo del fragmento "{0}".</target>
      </trans-unit>
    </body>
  </file>
