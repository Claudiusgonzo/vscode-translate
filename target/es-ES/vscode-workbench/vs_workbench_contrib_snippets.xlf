  <file original="src/vs/workbench/contrib/snippets/browser/snippets.contribution" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="snippetSchema.json.default">
        <source xml:lang="en">Empty snippet</source>
        <target state="translated">Fragmento de código vacío</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json">
        <source xml:lang="en">User snippet configuration</source>
        <target state="translated">Configuración de fragmento de código del usuario</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.prefix">
        <source xml:lang="en">The prefix to used when selecting the snippet in intellisense</source>
        <target state="translated">El prefijo que se debe usar al seleccionar el fragmento de código en Intellisense</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.body">
        <source xml:lang="en">The snippet content. Use '$1', '${1:defaultText}' to define cursor positions, use '$0' for the final cursor position. Insert variable values with '${varName}' and '${varName:defaultText}', e.g. 'This is file: $TM_FILENAME'.</source>
        <target state="translated">El contenido del fragmento. Utilice "$1", "${1:defaultText}" para definir las posiciones del cursor, utilice "$0" para la posición del cursor final. Inserte valores de variable con "${varName}" y "${varName:defaultText}", por ejemplo, "Este es el archivo: $TM_FILENAME".</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.description">
        <source xml:lang="en">The snippet description.</source>
        <target state="translated">La descripción del fragmento de código.</target>
      </trans-unit>
      <trans-unit id="snippetSchema.json.scope">
        <source xml:lang="en">A list of language names to which this snippet applies, e.g. 'typescript,javascript'.</source>
        <target state="translated">Una lista de nombres de lenguaje a los que se aplica este fragmento, por ejemplo, "typescript,javascript".</target>
      </trans-unit>
    </body>
  </file>
