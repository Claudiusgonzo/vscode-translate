  <file original="src/vs/workbench/contrib/update/browser/update" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="releaseNotes">
        <source xml:lang="en">Release Notes</source>
        <target state="translated">Notas de la versión</target>
      </trans-unit>
      <trans-unit id="showReleaseNotes">
        <source xml:lang="en">Show Release Notes</source>
        <target state="translated">Mostrar las notas de la versión</target>
      </trans-unit>
      <trans-unit id="read the release notes">
        <source xml:lang="en">Welcome to {0} v{1}! Would you like to read the Release Notes?</source>
        <target state="translated">{0} v{1}. ¿Quiere leer las notas de la versión?</target>
      </trans-unit>
      <trans-unit id="licenseChanged">
        <source xml:lang="en">Our license terms have changed, please click [here]({0}) to go through them.</source>
        <target state="translated">Los términos de licencia han cambiado. Haga clic [aquí]({0}) para revisarlos.</target>
      </trans-unit>
      <trans-unit id="updateIsReady">
        <source xml:lang="en">New {0} update available.</source>
        <target state="translated">Nueva actualización de {0} disponible.</target>
      </trans-unit>
      <trans-unit id="checkingForUpdates">
        <source xml:lang="en">Checking for Updates...</source>
        <target state="translated">Buscando actualizaciones...</target>
      </trans-unit>
      <trans-unit id="update service">
        <source xml:lang="en">Update Service</source>
        <target state="translated">Servicio de Actualización</target>
      </trans-unit>
      <trans-unit id="noUpdatesAvailable">
        <source xml:lang="en">There are currently no updates available.</source>
        <target state="translated">Actualmente, no hay actualizaciones disponibles.</target>
      </trans-unit>
      <trans-unit id="ok">
        <source xml:lang="en">OK</source>
        <target state="translated">Aceptar</target>
      </trans-unit>
      <trans-unit id="thereIsUpdateAvailable">
        <source xml:lang="en">There is an available update.</source>
        <target state="translated">Hay una actualización disponible.</target>
      </trans-unit>
      <trans-unit id="download update">
        <source xml:lang="en">Download Update</source>
        <target state="translated">Descargar actualización</target>
      </trans-unit>
      <trans-unit id="later">
        <source xml:lang="en">Later</source>
        <target state="translated">Más tarde</target>
      </trans-unit>
      <trans-unit id="updateAvailable">
        <source xml:lang="en">There's an update available: {0} {1}</source>
        <target state="translated">Hay una actualización disponible: {0} {1}</target>
      </trans-unit>
      <trans-unit id="installUpdate">
        <source xml:lang="en">Install Update</source>
        <target state="translated">Instalar la actualización </target>
      </trans-unit>
      <trans-unit id="updateInstalling">
        <source xml:lang="en">{0} {1} is being installed in the background; we'll let you know when it's done.</source>
        <target state="translated">{0} {1} se está instalando en segundo plano; le avisaremos cuando esté hecho.</target>
      </trans-unit>
      <trans-unit id="updateNow">
        <source xml:lang="en">Update Now</source>
        <target state="translated">Actualizar ahora</target>
      </trans-unit>
      <trans-unit id="updateAvailableAfterRestart">
        <source xml:lang="en">Restart {0} to apply the latest update.</source>
        <target state="translated">Reinicie {0} para aplicar la última actualización.</target>
      </trans-unit>
      <trans-unit id="checkForUpdates">
        <source xml:lang="en">Check for Updates...</source>
        <target state="translated">Buscar actualizaciones...</target>
      </trans-unit>
      <trans-unit id="DownloadingUpdate">
        <source xml:lang="en">Downloading Update...</source>
        <target state="translated">Descargando actualización...</target>
      </trans-unit>
      <trans-unit id="installUpdate...">
        <source xml:lang="en">Install Update...</source>
        <target state="translated">Instalar actualización...</target>
      </trans-unit>
      <trans-unit id="installingUpdate">
        <source xml:lang="en">Installing Update...</source>
        <target state="translated">Instalando actualización...</target>
      </trans-unit>
      <trans-unit id="restartToUpdate">
        <source xml:lang="en">Restart to Update (1)</source>
        <target state="translated">Reiniciar para actualizar (1)</target>
      </trans-unit>
    </body>
  </file>
