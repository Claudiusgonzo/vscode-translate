<?xml version="1.0" encoding="utf-8"?>
<xliff version="1.2" xmlns="urn:oasis:names:tc:xliff:document:1.2">
  <file original="build/win32/i18n/messages" source-language="en" target-language="es" datatype="plaintext">
    <body>
      <trans-unit id="AddContextMenuFiles">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer file context menu</source>
        <target state="translated">Agregar la acción "Abrir con %1" al menú contextual de archivo del Explorador de Windows</target>
      </trans-unit>
      <trans-unit id="AddContextMenuFolders">
        <source xml:lang="en">Add "Open with %1" action to Windows Explorer directory context menu</source>
        <target state="translated">Agregar la acción "Abrir con %1" al menú contextual de directorio del Explorador de Windows</target>
      </trans-unit>
      <trans-unit id="AssociateWithFiles">
        <source xml:lang="en">Register %1 as an editor for supported file types</source>
        <target state="translated">Registrar %1 como editor para tipos de archivo admitidos</target>
      </trans-unit>
      <trans-unit id="AddToPath">
        <source xml:lang="en">Add to PATH (requires shell restart)</source>
        <target state="translated">Agregar a PATH (requiere reinicio del shell)</target>
      </trans-unit>
      <trans-unit id="RunAfter">
        <source xml:lang="en">Run %1 after installation</source>
        <target state="translated">Ejecutar %1 después de la instalación</target>
      </trans-unit>
      <trans-unit id="Other">
        <source xml:lang="en">Other:</source>
        <target state="translated">Otros:</target>
      </trans-unit>
      <trans-unit id="SourceFile">
        <source xml:lang="en">%1 Source File</source>
        <target state="translated">Archivo de origen %1</target>
      </trans-unit>
      <trans-unit id="OpenWithCodeContextMenu">
        <source xml:lang="en">Open w&amp;ith %1</source>
        <target state="translated">Abrir c&amp;on %1</target>
      </trans-unit>
    </body>
  </file>
